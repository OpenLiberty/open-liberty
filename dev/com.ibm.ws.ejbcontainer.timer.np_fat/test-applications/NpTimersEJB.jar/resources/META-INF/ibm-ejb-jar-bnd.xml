    <session name="NpTimerBeanRequiresNew" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/NpTimerBeanRequiresNew"/>

    <session name="NpTimerBeanRequired" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/NpTimerBeanRequired"/>

    <session name="NpTimerBeanNotSupported" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/NpTimerBeanNotSupported"/>

    <session name="NpTimerBeanBMTNoCommit" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/NpTimerBeanBMTNoCommit"/>

    <session name="TimeoutFailureBeanCMT" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/TimeoutFailureBeanCMT"/>

    <session name="TimeoutFailureBeanBMT" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/TimeoutFailureBeanBMT"/>

    <session name="XMLTxBean" simple-binding-name="com/ibm/ws/ejbcontainer/timer/np/ejb/XMLTxBean"/>


