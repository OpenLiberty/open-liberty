<!DOCTYPE Stats SYSTEM "stats.dtd">

<Stats type="com.ibm.ws.webcontainer.pmi.webAppModuleStats">
    <description>webAppModule.desc</description>
    <resourceBundle>com.ibm.ws.pmi.properties.PMIText</resourceBundle>
    <CountStatistic  ID="1" name="webAppModule.numLoadedServlets">
        <level>low</level>
		<unit>unit.none</unit>   
        <description>webAppModule.numLoadedServlets.desc</description>
        <comment>Number of servlets that were loaded</comment>
        <statisticSet>basic</statisticSet>
    </CountStatistic>


    <CountStatistic  ID="2" name="webAppModule.numReloads">
        <level>low</level>
		<unit>unit.none</unit>   
        <description>webAppModule.numReloads.desc</description>
        <comment>Number of servlets that were reloaded</comment>
        <statisticSet>basic</statisticSet>
    </CountStatistic>


</Stats>
