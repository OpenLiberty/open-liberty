-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

# This really is a singleton bundle
Bundle-Name: z/OS Native Logging
Bundle-SymbolicName: com.ibm.ws.zos.logging
Bundle-Description: z/OS specific code in support of tracing and logging from native; version=${bVersion}

Bundle-Activator: com.ibm.ws.zos.logging.internal.ZosLoggingBundleActivator

WS-TraceGroup: zCore

#Export-Package: com.ibm.ws.zos.logging.console;provide:=true
Export-Package: com.ibm.ws.zos.logging;provide:=true

Private-Package: com.ibm.ws.zos.logging.console, \
  com.ibm.ws.zos.logging.internal, com.ibm.ws.zos.logging.internal.resources

-includeresource: \
 META-INF/MessageRouter.properties=resources/MessageRouter.properties, \
 OSGI-INF=resources/OSGI-INF

    
-dsannotations: \
  com.ibm.ws.zos.logging.internal.LoggingHardcopyLogHandler, \
  com.ibm.ws.zos.logging.console.LoggingCommandHandler, \
  com.ibm.ws.zos.logging.ZosLoggingConfiguration

-buildpath: \
	com.ibm.ws.kernel.boot.core;version=latest,\
	com.ibm.ws.kernel.service;version=latest,\
	com.ibm.ws.zos.command.processing;version=latest,\
	com.ibm.ws.zos.core;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
	com.ibm.websphere.org.osgi.service.cm;version=latest,\
	com.ibm.ws.logging.osgi;version=latest,\
	com.ibm.ws.logging.core;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	cglib:cglib-nodep;version=3.3.0, \
	com.ibm.ws.org.objenesis:objenesis;version=1.0, \
	com.ibm.ws.junit.extensions;version=latest, \
	com.ibm.ws.kernel.boot;version=latest, \
	com.ibm.ws.logging;version=latest, \
	org.hamcrest:hamcrest-all;version=1.3, \
	org.jmock:jmock-junit4;strategy=exact;version=2.5.1, \
	org.jmock:jmock;strategy=exact;version=2.5.1, \
	org.jmock:jmock-legacy;version=2.5.0
