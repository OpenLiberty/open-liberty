  <servlet name="FuelTypeListing">
  		<local-transaction boundary="BEAN_METHOD" resolver="CONTAINER_AT_BOUNDARY" unresolved-action="COMMIT" />
  </servlet>
  <servlet name="FuelType_include">
  		<local-transaction boundary="BEAN_METHOD" resolver="CONTAINER_AT_BOUNDARY" unresolved-action="COMMIT" />
  </servlet>
  
