-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 11
javac.target: 11

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=11))"

-include= jar:${fileuri;${repo;org.jboss.weld.se:weld-se-core;[5.1,5.2)}}!/META-INF/MANIFEST.MF,\
          jar:${fileuri;${repo;org.jboss.weld.environment:weld-environment-common;[5.1,5.2)}}!/META-INF/MANIFEST.MF,\
          bnd.overrides

Bundle-SymbolicName: io.openliberty.org.jboss.weld5.se; singleton:=true

-includeresource: \
  @${repo;org.jboss.weld.se:weld-se-core;[5.1,5.2)},\
  @${repo;org.jboss.weld.environment:weld-environment-common;[5.1,5.2)}

instrument.disabled: true

-buildpath: \
	org.jboss.weld.se:weld-se-core;version="[5.1,5.2)", \
	org.jboss.weld.environment:weld-environment-common;version="[5.1,5.2)", \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
