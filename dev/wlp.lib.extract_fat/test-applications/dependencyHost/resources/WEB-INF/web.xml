<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app id="WebApp_ID">
  <display-name>Dependency Host</display-name>
  <description>This is a servlet hosts test dependencies for test samples.</description>

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>Dependency Host Servlet</servlet-name>
    <display-name>Dependency Host Servlet</display-name>
    <description>Hosts sample dependencies</description>
    <servlet-class>web.DependencyHostServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>Dependency Host Servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
