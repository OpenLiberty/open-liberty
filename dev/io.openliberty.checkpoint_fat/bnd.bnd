	io.opentracing:opentracing-util;version='0.33.0',\
	io.opentracing:opentracing-noop;version='0.33.0',\
	io.jaegertracing:jaeger-core;version='1.6.0',\
	io.jaegertracing:jaeger-thrift;version='1.6.0',\
