  net.sourceforge.htmlunit:webdriver;version=2.6,\
  org.seleniumhq.webdriver:webdriver-common;version=0.9.7376,\
  org.brotli:dec;version=0.1.2,\
\
  io.openliberty.jakarta.servlet.6.0;version=latest, \
