-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


src: \
    fat/src,\
    test-applications/hibertest/src

fat.project: true
tested.features: el-3.0, persistence-3.0, servlet-5.0, cdi-3.0, beanValidation-3.0, enterprisebeanslite-4.0

