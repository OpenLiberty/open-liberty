-include= ~../cnf/resources/bnd/rejar.props

hibernateVersion=6.0.10.Final
bVersion=1.0
Bundle-SymbolicName: com.ibm.ws.org.hibernate.validator.cdi.${hibernateVersion}

# Something that wildfly/thorntail apparently uses but blows up on Liberty
Fragment-Host:

Import-Package: \
  org.apache.el,\
  *

Export-Package: \
  org.hibernate.validator.cdi.*;version="${hibernateVersion}"

Include-Resource: \
  @${repo;org.hibernate.validator:hibernate-validator-cdi;${hibernateVersion}}!/META-INF/services/*,\
  META-INF=resources/META-INF
