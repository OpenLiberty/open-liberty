-include= ~../cnf/resources/bnd/rejar.props

bVersion=1.0
Bundle-SymbolicName: com.ibm.ws.org.hibernate.validator.cdi

# Something that wildfly/thorntail apparently uses but blows up on Liberty
Fragment-Host:

Import-Package: \
  org.apache.el,\
  *

Export-Package: \
  org.hibernate.validator.cdi.*;version="6.1.1.Final"

Include-Resource: \
  @${repo;org.hibernate.validator:hibernate-validator-cdi;6.1.1.Final}!/META-INF/services/*,\
  META-INF=resources/META-INF

-cdiannotations: