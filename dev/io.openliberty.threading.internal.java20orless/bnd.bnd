-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version<=20))"

Bundle-Name: Java 20 and Below
Bundle-SymbolicName: io.openliberty.threading.internal.java20orless
Bundle-Description: Lack of Virtual Thread API access in Java 20 and below; version=${bVersion}

WS-TraceGroup: threading

Private-Package: \
  io.openliberty.threading.internal.java20orless.*

-dsannotations: io.openliberty.threading.internal.java20orless.VirtualThreadDisallowed

-buildpath: \
  com.ibm.websphere.appserver.spi.logging,\
  com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
  com.ibm.websphere.org.osgi.core,\
  com.ibm.websphere.org.osgi.service.component,\
  com.ibm.wsspi.org.osgi.service.component.annotations,\
  com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
  com.ibm.ws.threading;version=latest
