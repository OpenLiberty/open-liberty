<server>

	<featureManager>
		<feature>servlet-3.1</feature>
		<feature>componenttest-1.0</feature>
		<feature>timedexit-1.0</feature>
	</featureManager>

	<httpEndpoint id="defaultHttpEndpoint" host="*"
		httpPort="${bvt.prop.HTTP_default}"
		httpsPort="${bvt.prop.HTTP_default.secure}">
