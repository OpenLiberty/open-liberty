-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Apache CXF Runtime Simple Frontend
Bundle-Description: Apache CXF Runtime Simple Frontend, version 2.6.2
Bundle-SymbolicName: com.ibm.ws.org.apache.cxf.cxf.rt.frontend.simple.2.6.2

Export-Package: \
  org.apache.cxf.*;version=2.6.2

Import-Package: \
 javax.servlet.*;version="[0.0,4)", \
 !org.springframework.*;resolution:=optional;version="[2.5,4)", \
 !org.apache.aries.*;version="[0.3,2)", \
 !org.osgi.service.blueprint.*, \
 org.apache.cxf.*;version="[2.6.2,2.6.3)", \
 *

Include-Resource:\
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/META-INF/spring.handlers, \
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/META-INF/spring.schemas, \
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/schemas/**, \
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/META-INF/DEPENDENCIES, \
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/META-INF/LICENSE, \
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/META-INF/NOTICE, \
 @${repo;org.apache.cxf.cxf-rt-frontend-simple;2.6.2.ibm-s20170216-1739;EXACT}!/org/apache/cxf/**

# Remove the resources (blueprint/metadata configuration files) in the OSGI-INF directory
# As we have no plan to support that, will add these things back with NLS support in the future
# @lib/cxf-rt-frontend-simple-2.6.2.jar!/OSGI-INF/blueprint/**, \