-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: z/OS Native Diagnostics
Bundle-SymbolicName: com.ibm.ws.zos.diagnostics
Bundle-Description: z/OS specific code in support of diagnostics; version=${bVersion}

WS-TraceGroup: zDiagnostics

#Export-Package: com.ibm.ws.diagnostics.zos.svcdump;provide:=true

Private-Package: com.ibm.ws.diagnostics.zos.*

Service-Component: \
  com.ibm.ws.zos.diagnostics.SvcdumpCommandHandler; \
    provide:=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    implementation:=com.ibm.ws.diagnostics.zos.svcdump.SvcdumpCommandHandler; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    NativeServiceSVCDUMP='com.ibm.ws.zos.core.NativeService(&(native.service.name=SVCDUMP)(is.authorized=true))';\
    properties:="modify.filter.regex=(\(?i\)\(svcdump\).*),display.command.help=TRUE,service.vendor=IBM", \
  com.ibm.ws.zos.diagnostics.TdumpCommandHandler; \
    provide:=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    implementation:=com.ibm.ws.diagnostics.zos.tdump.TdumpCommandHandler; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    properties:="modify.filter.regex=(\(?i\)\(tdump\).*),display.command.help=TRUE,service.vendor=IBM", \
  com.ibm.ws.diagnostics.zos.network.NetstatInformation; \
    provide:=com.ibm.wsspi.logging.IntrospectableService; \
    implementation:=com.ibm.ws.diagnostics.zos.network.NetstatInformation; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.diagnostics.zos.process.ProcessDiagnostics; \
    provide:=com.ibm.wsspi.logging.IntrospectableService; \
    implementation:=com.ibm.ws.diagnostics.zos.process.ProcessInformation; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.diagnostics.ServerDumpCommandHandler; \
    provide:=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    implementation:=com.ibm.ws.diagnostics.zos.dump.ServerDumpCommandHandler; \
    libertyProcess=com.ibm.ws.kernel.LibertyProcess; \
    locationAdmin=com.ibm.wsspi.kernel.service.location.WsLocationAdmin; \
    properties:="modify.filter.regex=(\(?i\)\(dump\).*),display.command.help=TRUE,service.vendor=IBM", \
  com.ibm.ws.zos.diagnostics.JavacoreCommandHandler; \
    provide:=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    implementation:=com.ibm.ws.diagnostics.zos.javadump.JavacoreCommandHandler; \
    libertyProcess=com.ibm.ws.kernel.LibertyProcess; \
    properties:="modify.filter.regex=(\(?i\)\(javacore\).*),display.command.help=TRUE,service.vendor=IBM", \
  com.ibm.ws.zos.diagnostics.HeapdumpCommandHandler; \
    provide:=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    implementation:=com.ibm.ws.diagnostics.zos.javadump.HeapdumpCommandHandler; \
    libertyProcess=com.ibm.ws.kernel.LibertyProcess; \
    properties:="modify.filter.regex=(\(?i\)\(heapdump\).*),display.command.help=TRUE,service.vendor=IBM"

-buildpath: \
	com.ibm.ws.kernel.boot;version=latest,\
	com.ibm.ws.zos.command.processing;version=latest,\
	com.ibm.ws.zos.core;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
