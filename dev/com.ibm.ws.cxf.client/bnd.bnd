-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: CXF Client Async Wrapper and WrapperManager
Bundle-SymbolicName: com.ibm.ws.cxf.client
Bundle-Description: Async capability for CXF HttpConduit; version=${bVersion}

WS-TraceGroup: JAXRS

Export-Package: \
  com.ibm.ws.cxf.client, \
  com.ibm.ws.cxf.client.component

Import-Package: \
 org.apache.cxf.message, \
 *
 
-dsannotations: \
  com.ibm.ws.cxf.client.*

-buildpath: \
  org.apache.cxf:cxf-core,\
  com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
  com.ibm.wsspi.org.osgi.service.component.annotations