-include= ~../cnf/resources/bnd/rejar.props

bVersion=1.0

Bundle-SymbolicName: com.ibm.ws.io.smallrye.reactive.messaging-provider.config20; singleton:=true

-dsannotations-inherit: true
-dsannotations: com.ibm.ws.microprofile.reactive.messaging.OLReactiveMessagingExtension

Import-Package: \
	!io.vertx.*,\
	javax.enterprise.*;version='[1.1,3)',\
	org.eclipse.microprofile.config;version='[2,2.1)',\
	org.eclipse.microprofile.config.inject;version='[1.1,2)',\
	org.eclipse.microprofile.config.spi;version='[2, 3)',\
	*

Include-Resource: META-INF=resources/META-INF, \
    @${repo;io.smallrye.reactive:smallrye-reactive-messaging-provider;1.0.9.SNAPSHOT;EXACT}!/!META-INF/*

Private-Package: \
	com.ibm.ws.microprofile.reactive.messaging