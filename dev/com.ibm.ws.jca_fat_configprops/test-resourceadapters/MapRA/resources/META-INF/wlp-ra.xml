<connector xmlns="http://websphere.ibm.com/xml/ns/javaee">
  <resourceadapter>
    <!-- We don't currently support primitives. -->
<!--
    <config-property config-property-name="propInt1RA_WLP" default="10000"/>
    <config-property config-property-name="propInt1_Anno" default="10000"/>
    <config-property config-property-name="propInt1_DD" default="10000"/>
    <config-property config-property-name="propInt1_WLP" default="10000"/>
-->

    <config-property config-property-name="propInteger2RA_WLP" default="20000"/>
    <config-property config-property-name="propInteger2_Anno" default="20000"/>
    <config-property config-property-name="propInteger2_DD" default="20000"/>
    <config-property config-property-name="propInteger2_WLP" default="20000"/>

    <config-property config-property-name="propStringRA_WLP" default="RA wlp-ra.xml Default Value"/>
    <config-property config-property-name="propString_Anno" default="RA wlp-ra.xml Default Value"/>
    <config-property config-property-name="propString_DD" default="RA wlp-ra.xml Default Value"/>
    <config-property config-property-name="propString_WLP" default="RA wlp-ra.xml Default Value"/>

    <outbound-resourceadapter>
      <connection-definition connectionfactory-interface="java.util.Map">
        <!-- We don't currently support primitives. -->
<!--
        <config-property config-property-name="propInt1_WLP" default="11111"/>
-->
        <config-property config-property-name="propInteger2_WLP" default="22222"/>
        <config-property config-property-name="propString_WLP" default="MCF wlp-ra.xml Default Value"/>
      </connection-definition>
    </outbound-resourceadapter>
  </resourceadapter>
</connector>
