<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee web-app_3_0.xsd"
    id="WebApp_ID">

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>ConfigPropsRA Test Servlet</servlet-name>
    <display-name>ConfigProps RA Test Servlet</display-name>
    <servlet-class>web.ConfigPropsRATestServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>ConfigPropsRA Test Servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

  <!-- RESOURCE REFERENCES -->

</web-app>
