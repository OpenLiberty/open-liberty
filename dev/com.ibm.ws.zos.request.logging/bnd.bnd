-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Request Logging
Bundle-SymbolicName: com.ibm.ws.zos.request.logging
Bundle-Description: RequestLogging: Request Logging; version=${bVersion}

WS-TraceGroup: zosRequestLogging

Import-Package: com.ibm.ws.zos.request.logging.data, *

Export-Package: com.ibm.ws.zos.request.logging;provide:=true

Private-Package: com.ibm.ws.zos.request.logging.internal.*, \
    com.ibm.ws.zos.request.logging

-dsannotations: \
  com.ibm.ws.zos.request.logging.internal.SmfProbeExtensionImpl, \
  com.ibm.ws.zos.request.logging.internal.SmfDataRecorderImpl, \
  com.ibm.ws.zos.request.logging.internal.UserDataImpl

-buildpath: \
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.websphere.zos.request.logging;version=latest,\
	com.ibm.ws.kernel.boot.core;version=latest,\
	com.ibm.ws.kernel.service;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.ws.request.probes;version=latest,\
	com.ibm.ws.resource;version=latest,\
	com.ibm.ws.zos.core;version=latest,\
	com.ibm.ws.zos.request.logging.data;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest
