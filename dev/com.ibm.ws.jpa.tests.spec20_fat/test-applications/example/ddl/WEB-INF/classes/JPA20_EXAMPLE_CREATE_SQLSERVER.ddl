CREATE TABLE SimpleExample (id INT NOT NULL, boolean1 BIT, boolean2 BIT, boolean3 BIT, byte1 TINYINT, byte2 TINYINT, byte3 TINYINT, char1 INT, char2 INT, char3 INT, double1 FLOAT(32), double2 FLOAT(32), double3 FLOAT(32), float1 REAL, float2 REAL, float3 REAL, int1 INT, int2 INT, int3 INT, long1 BIGINT, long2 BIGINT, long3 BIGINT, short1 SMALLINT, short2 SMALLINT, short3 SMALLINT, str1 VARCHAR(255), str2 VARCHAR(255), str3 VARCHAR(255), PRIMARY KEY (id));