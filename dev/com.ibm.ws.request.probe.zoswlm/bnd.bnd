-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Zos Wlm Request Probe
Bundle-SymbolicName: com.ibm.ws.request.probe.zoswlm
Bundle-Description: ZosWlmRequestProbe: Zos Wlm Request Probe; version=${bVersion}

WS-TraceGroup: com.ibm.ws.request.probe.zoswlm

Import-Package: com.ibm.wsspi.probeExtension, \
    com.ibm.wsspi.request.probe.bci

Private-Package: com.ibm.ws.request.probe.zoswlm

instrument.disabled: true

-dsannotations: \
    com.ibm.ws.request.probe.zoswlm.WlmClassificationWlmRunWork, \
    com.ibm.ws.request.probe.zoswlm.EnclaveManagerProcessPostEnclaveDelete

-buildpath: \
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.request.probes;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest
