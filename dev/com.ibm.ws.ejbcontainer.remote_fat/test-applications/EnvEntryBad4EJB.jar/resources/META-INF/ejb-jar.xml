<?xml version="1.0"?>

<ejb-jar
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
  version="3.1"
>
  <enterprise-beans>
    <session>
      <ejb-name>Bad4XmlBean</ejb-name>
      <ejb-class>com.ibm.ws.ejbcontainer.remote.enventry.bad.ejb4.Bad4XmlBean</ejb-class>
      <session-type>Stateless</session-type>
       
      <env-entry>
        <description>E4x - Existing class that is neither an Enum nor a Class; specified in XML only</description>
        <env-entry-name>EnvEntry_ExistingNonEnumNonClass_EntryName</env-entry-name>
        <env-entry-type>com.ibm.ws.ejbcontainer.remote.enventry.bad.ejb4.Bad4XmlBean</env-entry-type>
        <env-entry-value>NOT_APPLICABLE</env-entry-value>
        <injection-target>
          <injection-target-class>com.ibm.ws.ejbcontainer.remote.enventry.bad.ejb4.Bad4XmlBean</injection-target-class>
          <injection-target-name>ivEnvEntry_ExistingNonEnumNonClass</injection-target-name>
        </injection-target>
      </env-entry>
    </session>

    <session>
      <ejb-name>Bad4AnnBean</ejb-name>
      <ejb-class>com.ibm.ws.ejbcontainer.remote.enventry.bad.ejb4.Bad4AnnBean</ejb-class>
      <session-type>Stateless</session-type>
       
      <env-entry>
        <description>E4a - Existing class that is neither an Enum nor a Class; specified in XML, with annotation</description>
        <env-entry-name>EnvEntry_ExistingNonEnumNonClass_EntryName</env-entry-name>
        <env-entry-type>com.ibm.ws.ejbcontainer.remote.enventry.bad.ejb4.Bad4AnnBean</env-entry-type>
        <env-entry-value>NOT_APPLICABLE</env-entry-value>
      </env-entry>
    </session>
  </enterprise-beans>
</ejb-jar>