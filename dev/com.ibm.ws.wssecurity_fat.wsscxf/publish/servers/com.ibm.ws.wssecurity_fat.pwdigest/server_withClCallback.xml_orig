<server>
    <featureManager>
        <feature>servlet-3.0</feature>
        <feature>appSecurity-2.0</feature>
        <feature>jsp-2.2</feature>
        <feature>jaxws-2.2</feature>
        <feature>wsSecurity-1.1</feature>
    </featureManager>

    <include location="../fatTestPorts.xml"/>

    <basicRegistry id="basic" realm="customRealm">
        <user name="user1" password="security" />
        <user name="user2" password="security" />
        <user name="user3" password="security" />
        <user name="user4" password="security" />       
        <user name="user5" password="security" />       
        <user name="user6" password="security" />
        <user name="user22" password="security" />
        <user name="user88" password="security" />
    </basicRegistry>     
    
    <sslDefault sslRef="DefaultSSLSettings" />
    <ssl id="DefaultSSLSettings"
         keyStoreRef="myKeyStore"
         trustStoreRef="myTrustStore"
         clientAuthenticationSupported="true" />
    <keyStore id="myKeyStore" password="Liberty" type="jks" location="${server.config.dir}/key.jks" />
    <keyStore id="myTrustStore" password="LibertyServer" type="jks" location="${server.config.dir}/sslServerTrust.jks" />
        
    <wsSecurityProvider id="default"
        ws-security.callback-handler="com.ibm.ws.wssecurity.fat.pwdigest.PWDigestCallbackHandler">
    </wsSecurityProvider>
    
    <wsSecurityClient id="default"
        ws-security.password="security"
        ws-security.username="user22"
        ws-security.callback-handler="com.ibm.ws.wssecurity.fat.pwdigest.ClientPWDigestCallbackHandler">
    </wsSecurityClient>
    
    
    <application name="pwdigest" location="${server.config.dir}/pwdigest.war">
      <classloader apiTypeVisibility="spec, ibm-api, third-party" />
    </application>
    <application name="pwdigestclient" location="${server.config.dir}/pwdigestclient.war">
      <classloader apiTypeVisibility="spec, ibm-api, third-party" />
    </application>
    <!--  -->
    
</server>

