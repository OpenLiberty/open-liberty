    <wsdl:binding name="UrnXmlEnc1SoapBinding" type="tns:XmlEnc1">
        <wsp:PolicyReference URI="#EncryptUNTReqPolicy" />
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
