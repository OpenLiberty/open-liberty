<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Fails To Start Web Application</display-name>
  <servlet>
    <display-name>FailsToStartServlet</display-name>
    <servlet-name>FailsToStartServlet</servlet-name>
    <servlet-class>com.ibm.ws.microprofile.health20.fails.to.start.health.check.app.FailsToStartServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>FailsToStartServlet</servlet-name>
    <url-pattern>/FailsToStartServlet</url-pattern>
  </servlet-mapping>
</web-app>