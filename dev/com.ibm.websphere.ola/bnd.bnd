-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: WebSphere Optimized Local Adapters APIs and SPIs
Bundle-SymbolicName: com.ibm.websphere.ola
Bundle-Description: WebSphere Optimized Local Adapters APIs and SPIs; version=${bVersion}

WS-TraceGroup: OLA

Export-Package: com.ibm.websphere.ola;provide:=true

instrument.disabled: true

-buildpath: \
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.javaee.connector.1.6;version=latest,\
	com.ibm.websphere.javaee.ejb.3.1;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
