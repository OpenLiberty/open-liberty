-include= ~../cnf/resources/bnd/rejar.props
bVersion=1.0
cxfVersion=3.2.6

Bundle-SymbolicName: com.ibm.ws.org.apache.cxf.cxf.rt.frontend.jaxws.3.2

Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.8))"

Import-Package: \
com.ibm.wsspi.classloading, \
org.apache.cxf.bindings.soap.*,\
!com.ctc.wstx.*,\
!org.codehaus.stax2.*,\
!com.sun.msv.*,\
!com.sun.xml.bind.marshaller,\
!com.sun.xml.fastinfoset.stax.*,\
!org.apache.log4j.*,\
!net.sf.cglib.proxy.*,\
!org.springframework.*,\
!javax.validation.*, \
!org.apache.aries.*,\
!org.apache.cxf.transport.https,\
!org.osgi.service.blueprint.*,\
javax.xml.bind.*;version="[2.3,3)",\
javax.xml.ws.*;version="[2.3,3)", \
*

DynamicImport-Package: com.ibm.ws.jaxws.client

Export-Package: \
  org.apache.cxf.jaxws.*,\
  org.apache.cxf.jaxws22.*

app-resources= \
  META-INF/services/javax.xml.ws.spi.Provider

