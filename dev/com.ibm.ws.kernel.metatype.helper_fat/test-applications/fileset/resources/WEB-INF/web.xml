<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app id="WebApp_ID">
  <display-name>fileset</display-name>
  <description>This is a servlet designed to exercise Fileset functionality.</description>

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>Fileset Servlet</servlet-name>
    <display-name>Fileset Servlet</display-name>
    <description></description>
    <servlet-class>com.ibm.ws.kernel.metatype.helper.test.app.FilesetTestServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>Fileset Servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
