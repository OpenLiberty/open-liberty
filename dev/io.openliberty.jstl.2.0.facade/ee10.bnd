Bundle-SymbolicName: io.openliberty.tags.3.0.facade
Bundle-Description: facade to provide a classloader for our internal jstl classes for osgi application jsp backwards compatibility; version=${bVersion}

# Must provide same capability as the com.ibm.ws.jsp.jstl.facade bundle
Provide-Capability: com.ibm.ws.jsp.jstl.facade

# No transformation required 
# Updated the jakarta.el import to include Expression Language 6.0 (EE11) because Jakarta Tags is not changing for EE11.
Import-Package: \
  jakarta.el;version="[5.0,7.0)", \
  jakarta.servlet.jsp;version="[3.1,5.0)", \
  jakarta.servlet.jsp.el;version="[3.1,5.0)", \
  jakarta.servlet.jsp.resources;version="[3.1,5.0)", \
  jakarta.servlet.jsp.tagext;version="[3.1,5.0)", \
  jakarta.servlet.jsp.jstl.core;version="[3.0,4.0)", \
  jakarta.servlet.jsp.jstl.fmt;version="[3.0,4.0)", \
  jakarta.servlet.jsp.jstl.sql;version="[3.0,4.0)", \
  jakarta.servlet.jsp.jstl.tlv;version="[3.0,4.0)", \
  org.apache.taglibs.standard.functions;version="[3.0,4.0)", \
  org.apache.taglibs.standard.lang.jstl;version="[3.0,4.0)", \
  org.apache.taglibs.standard.lang.jstl.parser;version="[3.0,4.0)", \
  org.apache.taglibs.standard.lang.support;version="[3.0,4.0)", \
  org.apache.taglibs.standard.resources;version="[3.0,4.0)", \
