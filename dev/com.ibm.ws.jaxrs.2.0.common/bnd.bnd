 junit.framework.*;resolution:=optional, \
# com.sun.xml.fastinfoset.*;resolution:=optional, \
 org.apache.xml.resolver.*;resolution:=optional, \
 !org.junit.*;resolution:=optional, \
 !org.apache.velocity.*, \
