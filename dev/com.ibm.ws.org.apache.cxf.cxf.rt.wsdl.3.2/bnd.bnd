-include=~../cnf/resources/bnd/bundle.props

-sub: *.bnd

bVersion=1.0
cxfImportVersion=3.5.5
cxfExportVersion=3.4.3

Import-Package: \
  !com.ctc.wstx.*, \
  !org.codehaus.stax2.*, \
  !com.sun.msv.*, \
  !com.sun.xml.bind.marshaller, \
  !com.sun.xml.fastinfoset.stax.*, \
  !org.apache.log4j.*, \
  !net.sf.cglib.proxy.*, \
  !org.springframework.*, \
  !javax.validation.*, \
  !org.apache.aries.*, \
  !org.apache.cxf.transport.https, \
  !org.osgi.service.blueprint.*, \
  javax.xml.bind;version=!, \
  javax.xml.bind.annotation;version=!, \
  javax.xml.bind.annotation.adapters;version=!, \
  javax.xml.bind.attachment;version=!, \
  *

Export-Package: \
  org.apache.cxf.wsdl;version=${cxfExportVersion}, \
  org.apache.cxf.wsdl.binding;version=${cxfExportVersion}, \
  org.apache.cxf.wsdl.interceptors;version=${cxfExportVersion}, \
  org.apache.cxf.wsdl.service;version=${cxfExportVersion}, \
  org.apache.cxf.wsdl.service.factory;version=${cxfExportVersion}, \
  org.apache.cxf.wsdl11.*;version=${cxfExportVersion}

-includeresource: \
  @${repo;org.apache.cxf:cxf-rt-wsdl;${cxfImportVersion};EXACT}!/!*-INF/*, \
  META-INF/cxf=resources/META-INF/cxf, \
  org/apache/cxf=${bin}/org/apache/cxf

instrument.ffdc: false
instrument.classesIncludes: \
  org/cxf/apache/**/*.class

-buildpath: \
  org.apache.cxf:cxf-rt-wsdl;strategy=exact;version=${cxfImportVersion}, \
  com.ibm.ws.org.apache.cxf.cxf.core.3.2;version=latest, \
  com.ibm.websphere.org.osgi.core, \
  com.ibm.ws.org.osgi.annotation.versioning;version=latest, \
  com.ibm.ws.logging.core, \
  com.ibm.ws.org.apache.ws.xmlschema.core.2.0.3, \
  com.ibm.websphere.javaee.wsdl4j.1.2
