-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 1.8
javac.target: 1.8

Bundle-Name: Private OpenAPI
Bundle-SymbolicName: com.ibm.ws.openapi.3.1.private
Bundle-Description: Project for Private OpenAPI; version=${bVersion}

Web-ContextPath: @privateOpenAPIDocsURL

IBM-Authorization-Roles: com.ibm.ws.management

WS-TraceGroup: OpenAPI

Include-Resource: \
    WEB-INF=resources/WEB-INF

Private-Package: \
	com.ibm.ws.openapi31.privateapi,\
	com.ibm.ws.openapi31.privateapi.resources.*

-buildpath: \
	com.ibm.websphere.org.osgi.core,\
	com.ibm.websphere.javaee.servlet.3.1;version=latest,\
	com.ibm.ws.logging.core;version=latest,\
	com.ibm.ws.openapi.3.1;version=latest
