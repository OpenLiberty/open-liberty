-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: componenttest.app.2.0

Export-Package: \
  componenttest.app;version=1.0.16

# componenttest.app depends on javax.servlet, but we don't want to include it in
# the componenttest-2.0 feature since we want tests to fail if a product feature
# is actually missing javax.servlet, so we use a dynamic import instead.
Import-Package: \
  !jakarta.servlet.*, \
  *

DynamicImport-Package: \
  jakarta.servlet, \
  jakarta.servlet.http
