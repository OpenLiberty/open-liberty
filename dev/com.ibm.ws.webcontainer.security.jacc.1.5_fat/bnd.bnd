-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src: fat/src

fat.project: true

tested.features: audit-2.0, jacc-1.5, expressionLanguage-4.0, appsecurity-4.0, servlet-5.0, cdi-3.0, pages-3.0, appAuthorization-2.0, \
