<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0">

  <display-name>MangedBeanTest</display-name>
  
  <env-entry>
    <env-entry-name>myInt</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>16</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>myStr</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>HelloYou</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>myBeanName</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>runner</env-entry-value>
  </env-entry>

</web-app>