-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.javaee.jaxws.2.2; singleton:=true

Export-Package: \
  javax.jws.*; version="1.0",\
  javax.xml.soap; version="1.4",\
  javax.xml.ws.*; version="2.2"

Import-Package: \
  javax.xml.bind.*; version="[2.2,3)", \
  *

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	com.ibm.ws.javax.j2ee:jaxws-api;version=2.2,\
	javax.jws:jsr181-api;version=latest,\
	javax.xml.soap:javax.xml.soap-api;version=1.4.0
