-include= ~../cnf/resources/bnd/bundle.props

bVersion: 1.0

Bundle-Name: Open Liberty Checkpoint Support for Open J9
Bundle-Description: Open Liberty Checkoint Support for Open J9, version ${bVersion}
Bundle-SymbolicName: io.openliberty.checkpoint.openj9
Private-Package: io.openliberty.checkpoint.openj9.internal
Import-Package: org.eclipse.openj9.criu; resolution:=optional, *
Bundle-Activator: io.openliberty.checkpoint.openj9.internal.ExecuteCRIU_OpenJ9

-buildpath: \
	io.openliberty.checkpoint;version=latest,\
	io.openliberty.org.eclipse.openj9.criu,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.ws.logging.core, \
	com.ibm.wsspi.org.osgi.service.component;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
	com.ibm.websphere.org.osgi.core,\
	com.ibm.ws.kernel.boot.core;version=latest



