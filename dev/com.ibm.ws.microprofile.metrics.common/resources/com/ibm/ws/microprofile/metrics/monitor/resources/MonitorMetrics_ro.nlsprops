#CMVCPATHNAME N/A
#COMPONENTPREFIX CWPMI
#COMPONENTNAMEFOR CWPMI WebSphere Performance Monitoring MicroProfile Metrics API
#NLS_ENCODING=UNICODE
#NLS_MESSAGEFORMAT_NONE
#ISMESSAGEFILE true
