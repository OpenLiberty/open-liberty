-include=~../cnf/resources/bnd/bundle.props

-sub: *.bnd

bVersion=1.0
cxfImportVersion=3.5.5
cxfExportVersion=3.4.1

WS-TraceGroup: WSSECURITY

Bundle-ActivationPolicy: lazy

Import-Package:  \
  !org.apache.cxf.ws.mex.*, \
  !com.ctc.wstx.*, \
  !org.codehaus.stax2.*, \
  !net.sf.ehcache.*, \
  org.apache.neethi, \
  org.apache.neethi.builders, \
  org.apache.wss4j.*, \
  org.w3c.dom, \
  com.ibm.websphere.ras, \
  com.ibm.websphere.ras.annotation, \
  com.ibm.ws.ffdc, \
  javax.servlet.*;version="[0.0.0,3.0.0)", \
  *

Export-Package: \
  org.apache.cxf.ws.security.sts.provider.operation;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.wss4j.policyvalidators;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.sts.provider;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.policy.builders;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.sts.provider.model;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.trust.delegation;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.sts.provider.model.wstrust14;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.policy;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.cache;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.policy.model;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.policy.custom;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.wss4j.policyhandlers;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.sts.provider.model.secext;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.trust;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.kerberos;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.policy.interceptors;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.sts.provider.model.xmldsig;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.sts.provider.model.utility;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.wss4j;version=${cxfExportVersion}, \
  org.apache.cxf.ws.security.tokenstore;version=${cxfExportVersion}

DynamicImport-Package: \
  org.apache.cxf.*, \
  com.ctc.wstx.*

-includeresource: \
  @${repo;org.apache.cxf:cxf-rt-ws-security;${cxfImportVersion};EXACT}!/!META-INF/maven/*, \
  @${repo;org.apache.cxf:cxf-rt-ws-security;${cxfImportVersion};EXACT}!/META-INF/DEPENDENCIES, \
  @${repo;org.apache.cxf:cxf-rt-ws-security;${cxfImportVersion};EXACT}!/META-INF/LICENSE, \
  @${repo;org.apache.cxf:cxf-rt-ws-security;${cxfImportVersion};EXACT}!/META-INF/NOTICE, \
  @${repo;org.apache.cxf:cxf-rt-ws-security;${cxfImportVersion};EXACT}!/META-INF/cxf/*, \
  META-INF/cxf/bus-extensions.txt=resources/META-INF/cxf/bus-extensions.txt, \
  cxf-ehcache.xml=resources/cxf-ehcache.xml, \
  org/apache/cxf=${bin}/org/apache/cxf

globalize: false
instrument.ffdc: true
instrument.classesIncludes: \
  org/apache/cxf/binding/xml/interceptor/XMLFaultInInterceptor.class, \
  org/apache/cxf/ws/security/wss4j/AbstractUsernameTokenAuthenticatingInterceptor.class, \
  org/apache/cxf/ws/security/wss4j/AbstractWSS4JInterceptor.class, \
  org/apache/cxf/ws/security/wss4j/PolicyBasedWSS4JInInterceptor.class, \
  org/apache/cxf/ws/security/wss4j/PolicyBasedWSS4JOutInterceptor.class, \
  org/apache/cxf/ws/security/wss4j/WSS4JInInterceptor.class, \
  org/apache/cxf/ws/security/wss4j/WSS4JOutInterceptor.class

-buildpath: \
  org.apache.cxf:cxf-rt-ws-security;strategy=exact;version=${cxfImportVersion}, \
  com.ibm.ws.org.apache.cxf.cxf.core.3.2;version=latest, \
  com.ibm.ws.org.apache.cxf.cxf.rt.bindings.soap.3.2;version=latest, \
  com.ibm.ws.org.apache.cxf.cxf.rt.ws.policy.3.2;version=latest, \
  com.ibm.ws.org.apache.cxf.cxf.rt.wsdl.3.2;version=latest, \
  com.ibm.ws.org.apache.cxf.rt.security.3.4.1;version=latest, \
  com.ibm.ws.org.apache.cxf.rt.security.saml.3.4.1;version=latest, \
  com.ibm.ws.org.apache.neethi.3.1.1;version=latest, \
  com.ibm.ws.org.apache.wss4j.policy;version=latest, \
  com.ibm.ws.org.apache.wss4j.ws.security.common;version=latest, \
  com.ibm.ws.org.apache.wss4j.ws.security.dom;version=latest, \
  com.ibm.ws.org.apache.wss4j.ws.security.stax;version=latest, \
  com.ibm.ws.org.apache.santuario.xmlsec.2.2.0;version=latest, \
  com.ibm.ws.org.ehcache.ehcache.107.3.8.1;version=latest, \
  com.ibm.ws.org.opensaml.opensaml.core.3.4.5;version=latest, \
  com.ibm.websphere.appserver.spi.logging, \
  org.codehaus.woodstox:stax2-api;version='4.2', \
  org.codehaus.woodstox:woodstox-core-asl;version='4.2.0'
