  org.jboss.resteasy.core.messagebody;version=4.5.2.Final, \
  org.jboss.resteasy.core.providerfactory;version=4.5.2.Final;thread-context=true, \
  org.jboss.resteasy.core.registry;version=4.5.2.Final, \
