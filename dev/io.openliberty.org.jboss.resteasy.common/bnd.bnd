  org.jboss.resteasy.client.jaxrs.internal.proxy;version=4.5.2.Final, \
  org.jboss.resteasy.client.jaxrs.internal;version=4.5.2.Final;thread-context=true, \
  org.jboss.resteasy.client.jaxrs;version=4.5.2.Final;thread-context=true, \
