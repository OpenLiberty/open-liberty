	<ldapRegistry id="LDAP" realm="defaultWIMFileBasedRealm"
		host="vmm-mad1.rtp.raleigh.ibm.com" port="389" ignoreCase="true"
		baseDN="CN=Users,DC=vmm,DC=com"
		bindDN="cn=administrator,cn=users,dc=vmm,dc=com"
		bindPassword="password" ldapType="Microsoft Active Directory"
		sslEnabled="false">

		<activedFilters
			userFilter="(&amp;(sAMAccountName=%v)(objectcategory=user))"
			groupFilter="(&amp;(cn=%v)(objectcategory=group))"
			userIdMap="user:sAMAccountName" groupIdMap="*:cn"
			groupMemberIdMap="memberOf:member">
		</activedFilters>

	</ldapRegistry>
    <federatedRepository>
        <primaryRealm name="newRealm">
            <participatingBaseEntry name="dc=rtp,dc=raleigh,dc=ibm,dc=com"/>
            <participatingBaseEntry name="cn=users,dc=secfvt2,dc=austin,dc=ibm,dc=com"/>
        </primaryRealm>
    </federatedRepository>
