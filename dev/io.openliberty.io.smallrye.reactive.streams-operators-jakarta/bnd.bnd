-include= ~../cnf/resources/bnd/rejar.props

bVersion=1.0

javac.source: 11
javac.target: 11

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=11))"

Bundle-SymbolicName: io.openliberty.io.smallrye.reactive.streams-operators-jakarta; singleton:=true

Export-Package: \
  io.smallrye.mutiny.jakarta.streams; version="2.5.6", \
  io.smallrye.mutiny.jakarta.streams.operators; version="2.5.6", \
  io.smallrye.mutiny.jakarta.streams.spi; version="2.5.6", \
  io.smallrye.mutiny.jakarta.streams.stages; version="2.5.6", \
  io.smallrye.mutiny.jakarta.streams.utils; version="2.5.6"

WS-TraceGroup: REACTIVESTREAMS

instrument.disabled: true

-buildpath: \
  io.openliberty.io.smallrye.reactive.mutiny;version=latest, \
  io.smallrye.reactive:mutiny-reactive-streams-operators-jakarta;version=2.5.6
