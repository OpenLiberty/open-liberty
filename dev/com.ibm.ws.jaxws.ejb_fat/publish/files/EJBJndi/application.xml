<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="6"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd">
    <initialize-in-order>true</initialize-in-order>

    <module>
        <ejb>EJBJndiEJB.jar</ejb>
    </module>

    <module>
        <web>
            <web-uri>EJBJndiWeb.war</web-uri>
            <context-root>EJBJndiWeb</context-root>
        </web>
    </module>

    <module>
        <web>
            <web-uri>EJBJndiWebEJB.war</web-uri>
            <context-root>EJBJndiWebEJB</context-root>
        </web>
    </module>
</application>
