-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src: \
    fat/src

fat.project: true
fat.test.databases: true
tested.features: persistence-3.1, persistenceContainer-3.1, enterpriseBeansLite-4.0, servlet-6.0, jdbc-4.2, jndi-1.0, xmlbinding-4.0, checkpoint-1.0

javac.source: 11
javac.target: 11

fat.minimum.java.level: 11

# Dependencies may be local bundles (e.g. com.ibm.websphere.javaee.servlet.3.1)
#      or binaries from Artifactory (e.g. commons-logging:commons-logging)
-buildpath: \
    com.ibm.ws.jpa_testframework;version=latest,\
    com.ibm.ws.jpa.tests.container.checkpoint_fat.common;version=latest,\
    fattest.simplicity;version=latest,\
    io.openliberty.jakarta.annotation.2.1;version=latest,\
    io.openliberty.jakarta.enterpriseBeans.4.0;version=latest,\
    io.openliberty.jakarta.servlet.6.0;version=latest,\
    io.openliberty.jakarta.persistence.3.1;version=latest,\
    io.openliberty.jakarta.transaction.2.0;version=latest,\
    io.openliberty.org.testcontainers;version=latest
