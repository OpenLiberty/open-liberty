-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


src: \
    fat/src, \
    test-applications/javatest/src

fat.project: true
tested.features: jdbc-4.0, beanvalidation-1.1, jpa-2.0, el-3.0

# Dependencies may be local bundles (e.g. com.ibm.websphere.javaee.servlet.3.1)
#      or binaries from an archive repository like Maven (e.g. commons-logging:commons-logging)
-buildpath: \
    com.ibm.ws.jpa_testframework;version=latest,\
    com.ibm.websphere.javaee.annotation.1.2;version=latest,\
    com.ibm.websphere.javaee.ejb.3.2;version=latest,\
    com.ibm.websphere.javaee.jms.2.0;version=latest,\
    com.ibm.websphere.javaee.servlet.4.0;version=latest,\
    com.ibm.websphere.javaee.persistence.2.2;version=latest,\
    io.openliberty.jakarta.annotation.2.1;version=latest,\
    io.openliberty.jakarta.enterpriseBeans.4.0;version=latest,\
    io.openliberty.jakarta.persistence.3.1;version=latest,\
    io.openliberty.jakarta.servlet.6.0;version=latest,\
    io.openliberty.jakarta.transaction.2.0;version=latest,\
    com.ibm.websphere.javaee.transaction.1.2;version=latest
