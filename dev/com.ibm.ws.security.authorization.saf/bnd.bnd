-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.2

Bundle-Name: WebSphere SAF Authorization Service
Bundle-SymbolicName: com.ibm.ws.security.authorization.saf
Bundle-Description: WebSphere SAF Authorization Service; version=${bVersion}
Bundle-ActivationPolicy: lazy

Export-Package: \
    com.ibm.wsspi.security.authorization.saf;provide:=true, \
    com.ibm.ws.security.authorization.saf;provide:=true

Private-Package: com.ibm.ws.security.authorization.saf.internal.*

Include-Resource: \
    OSGI-INF=resources/OSGI-INF

-dsannotations: \
  com.ibm.ws.security.authorization.saf.internal.SAFAuthorizationServiceImpl,\
  com.ibm.ws.security.authorization.saf.internal.SAFRoleMapperImpl

-buildpath: \
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
	com.ibm.ws.security;version=latest,\
	com.ibm.ws.security.authentication;version=latest,\
	com.ibm.ws.security.authentication.builtin;version=latest,\
	com.ibm.ws.security.authorization;version=latest,\
        com.ibm.ws.security.credentials,\
	com.ibm.ws.security.credentials.saf;version=latest,\
	com.ibm.ws.zos.core;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.ws.kernel.boot.core;version=latest

instrument.classesExcludes: com/ibm/ws/security/authorization/saf/internal/resources/SAFAuthorizationMessages*.class

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	cglib:cglib-nodep;version=3.3.0, \
	com.ibm.ws.org.objenesis:objenesis;version=1.0, \
	com.ibm.ws.junit.extensions;version=latest, \
	com.ibm.ws.kernel.boot;version=latest, \
	com.ibm.ws.logging;version=latest,\
	org.hamcrest:hamcrest-all;version=1.3, \
	org.jmock:jmock-junit4;strategy=exact;version=2.5.1, \
	org.jmock:jmock;strategy=exact;version=2.5.1, \
	org.jmock:jmock-legacy;version=2.5.0	
