<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee web-app_3_0.xsd"
    id="WebApp_ID">

  <display-name>JCA and JMS MDB example</display-name>
  <description>This servlet includes an application that uses an MDB</description>

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>JCA JMS BVT Servlet</servlet-name>
    <display-name>JCA and JMS BVT Servlet</display-name>
    <servlet-class>test.jca.jms.example.web.JCAStoreServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>JCA JMS BVT Servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
