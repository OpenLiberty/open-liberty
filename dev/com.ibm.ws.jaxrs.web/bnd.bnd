-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: JAX-RS Web
Bundle-SymbolicName: com.ibm.ws.jaxrs.web
Bundle-Description: IBM JAX-RS Web support; version=${bVersion}

WS-TraceGroup: JAXRS

Import-Package: \
   com.ibm.wsspi.adaptable.module.adapters, \
   com.ibm.wsspi.webcontainer.collaborator, \
   com.ibm.ws.container.service.metadata, \
   javax.ws.rs.*;version="[2.0,3)", \
   *

-dsannotations: com.ibm.ws.jaxrs.web.JaxRsInjectionClassListCollaborator
-dsannotations-inherit: true

instrument.classesExcludes: com/ibm/ws/jaxrs20/web/internal/resources/*.class

-buildpath: \
	com.ibm.websphere.appserver.spi.anno,\
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.websphere.javaee.jaxrs.2.0;version=latest,\
	com.ibm.ws.adaptable.module;version=latest,\
	com.ibm.ws.anno;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.webcontainer;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations
	
