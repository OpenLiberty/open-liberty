<?xml version="1.0"?>

<ejb-jar
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
  version="3.1"
>
  <enterprise-beans>
 
     <session>
       <ejb-name>Bad7Bean</ejb-name>
       <ejb-class>com.ibm.bnd.lookupoverride.doaApp7.ejb.Bad7Bean</ejb-class>
       <session-type>Stateless</session-type>
       <ejb-local-ref>
         <ejb-ref-name>SLT9</ejb-ref-name>
         <ejb-link>SimpleLookupTargetBean2</ejb-link>
      </ejb-local-ref>
    </session>
    
    <session>
       <ejb-name>TargetBean</ejb-name>
       <ejb-class>com.ibm.bnd.lookupoverride.shared.TargetBean</ejb-class>
       <session-type>Stateless</session-type>
    </session>
    
   
  </enterprise-beans>
  

  
</ejb-jar>
