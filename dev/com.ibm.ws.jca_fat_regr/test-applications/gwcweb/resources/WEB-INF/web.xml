<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee web-app_3_0.xsd"
    id="WebApp_ID">

  <display-name>JCAGWCWeb</display-name>

  <!-- SERVLET DEFINITIONS & MAPPINGS -->
  <servlet id="Default">
    <servlet-name>GWCTestServlet</servlet-name>
    <display-name>JCA GWC FAT REGR Servlet</display-name>
    <servlet-class>web.GWCTestServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>    
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>GWCTestServlet</servlet-name>
    <url-pattern>/GWCTestServlet</url-pattern>
  </servlet-mapping>
  
  <!--  END SERVLET DEFINITIONS & MAPPINGS -->

</web-app>
