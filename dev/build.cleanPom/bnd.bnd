-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


Bundle-Name: BND Pom cleaner
Bundle-SymbolicName: build.cleanPom
Bundle-Description: BND Pom cleaner; version=${bVersion}
Main-Class: io.openliberty.process.PostProcessBNDPom

Conditional-Package: \
			org.apache.maven.*, \
			org.json.simple.*, \
			org.codehaus.plexus.*


instrument.disabled: true

publish.wlp.jar.disabled: true


-buildpath: \
  org.apache.maven:maven-artifact;version=3.6.3, \
  org.apache.maven:maven-model;version=3.6.3, \
  org.apache.maven.indexer:indexer-core;version=6.2.2, \
  org.codehaus.plexus:plexus-utils;version=3.2.1, \
  com.googlecode.json-simple:json-simple;version=1.1.1
  

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file