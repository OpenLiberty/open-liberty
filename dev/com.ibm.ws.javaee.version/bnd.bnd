bVersion=1.0

Bundle-Name: Version property holders
Bundle-SymbolicName: com.ibm.ws.javaee.version
Bundle-Description: This bundle provides holders for version levels, version ${bVersion}

Export-Package: \
    com.ibm.ws.javaee.version

-buildpath: \
    com.ibm.websphere.org.osgi.core, \
    com.ibm.ws.org.osgi.annotation.versioning;version=latest
