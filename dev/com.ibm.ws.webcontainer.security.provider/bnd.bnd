-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Webcontainer Security Provider 
Bundle-Description: Webcontainer Security Provider Support, version=${bVersion}
Bundle-SymbolicName: com.ibm.ws.webcontainer.security.provider

-dsannotations: com.ibm.ws.webcontainer.security.internal.extended.WebAuthenticatorFactoryImplExtended
  
Export-Package: \
  com.ibm.ws.webcontainer.security.oauth20, \
  com.ibm.ws.webcontainer.security.openid20, \
  com.ibm.ws.webcontainer.security.extended, \
  com.ibm.ws.webcontainer.security.openidconnect

Import-Package: com.ibm.ws.webcontainer.security.internal, !com.ibm.ws.kernel.boot.cmdline, *

Private-Package: \
  com.ibm.ws.webcontainer.security.internal.extended

-buildpath: \
	com.ibm.websphere.org.osgi.core,\
	com.ibm.websphere.org.osgi.service.component,\
	com.ibm.websphere.javaee.servlet.3.0;version=latest,\
	com.ibm.ws.security;version=latest,\
	com.ibm.ws.security.authentication;version=latest,\
	com.ibm.ws.kernel.security.thread,\
	com.ibm.ws.security.authorization;version=latest,\
	com.ibm.ws.security.registry;version=latest,\
	com.ibm.ws.common.encoder;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.adaptable.module;version=latest,\
	com.ibm.ws.webcontainer;version=latest,\
	com.ibm.ws.webcontainer.security;version=latest,\
	com.ibm.ws.security.credentials;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.ws.security.authentication.tai;version=latest,\
	com.ibm.websphere.javaee.annotation.1.1;version=latest,\
	com.ibm.ws.javaee.dd;version=latest,\
	com.ibm.ws.javaee.dd.common;version=latest,\
	com.ibm.ws.anno;version=latest,\
	com.ibm.ws.dynacache;version=latest,\
	com.ibm.ws.security.jaas.common;version=latest,\
	com.ibm.ws.security.token;version=latest,\
	com.ibm.ws.transport.http;version=latest,\
	com.ibm.ws.kernel.service;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.security.audit.utils;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	org.hamcrest:hamcrest-all;version=1.3, \
	org.jmock:jmock-junit4;strategy=exact;version=2.5.1, \
	org.jmock:jmock;strategy=exact;version=2.5.1, \
	org.jmock:jmock-legacy;version=2.5.0, \
	com.ibm.ws.org.objenesis:objenesis;version=1.0, \
	cglib:cglib-nodep;version=2.1.0.3, \
	com.ibm.ws.junit.extensions;version=latest, \
	org.apache.httpcomponents:httpclient;version=4.1.2, \
	org.apache.httpcomponents:httpcore;version=4.1.2, \
	commons-logging:commons-logging;version=1.1.1, \
	commons-codec:commons-codec;version=1.4, \
	com.ibm.ws.kernel.boot;version=latest, \
	com.ibm.websphere.org.osgi.service.cm, \
	com.ibm.ws.injection;version=latest, \
	com.ibm.ws.org.apache.commons.io;version=latest, \
	com.ibm.ws.org.apache.commons.fileupload;version=latest
