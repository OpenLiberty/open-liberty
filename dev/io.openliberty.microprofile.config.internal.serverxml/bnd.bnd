-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


Bundle-Name: io.openliberty.microprofile.config.internal.serverxml
Bundle-SymbolicName: io.openliberty.microprofile.config.internal.serverxml

Bundle-Description:MicroProfile Configuration server XML config sources, version ${bVersion}

Export-Package: io.openliberty.microprofile.config.internal.serverxml

Import-Package: \
        org.eclipse.microprofile.config;version="[1.0,3)", \
        org.eclipse.microprofile.config.inject;version="[1.0,3)", \
        org.eclipse.microprofile.config.spi;version="[1.0,3)", \
        *
        
Include-Resource: \
  OSGI-INF=resources/OSGI-INF

WS-TraceGroup: APPCONFIG

-dsannotations: \
        io.openliberty.microprofile.config.internal.serverxml.AppPropertiesTrackingComponent, \
        io.openliberty.microprofile.config.internal.serverxml.AppPropertiesComponent

-buildpath: \
	com.ibm.websphere.org.eclipse.microprofile.config.1.1;version=latest, \
	com.ibm.ws.org.apache.commons.lang3;version=latest, \
	com.ibm.ws.logging;version=latest, \
	org.eclipse.osgi;version=latest, \
	com.ibm.websphere.org.osgi.core;version=latest, \
	com.ibm.websphere.org.osgi.service.component;version=latest, \
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest, \
	com.ibm.ws.kernel.service;version=latest, \
	com.ibm.websphere.javaee.annotation.1.2;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest, \
	com.ibm.websphere.org.osgi.service.cm;version=latest,\
	com.ibm.ws.cdi.interfaces;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.app.manager;version=latest,\
	com.ibm.ws.config;version=latest, \
	io.openliberty.microprofile.config.internal.common;version=latest
