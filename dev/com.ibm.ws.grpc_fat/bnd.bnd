-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

grpcVersion=1.57.2
nettyVersion=4.1.94.Final


src: \
    fat/src,\
    test-applications/HelloWorldClient.war/src,\
    test-applications/HelloWorldService.war/src, \
    test-applications/FavoriteBeerService.war/src, \
    test-applications/StreamingService.war/src, \
    test-applications/InvalidService.war/src, \
    test-applications/StoreProducerApp.war/src, \
    test-applications/StoreConsumerApp.war/src, \
    test-applications/StoreApp.war/src

fat.project: true

# Define additional tested features that are NOT present in any XML files in this bucket.
tested.features:\
	servlet-4.0,\
	jaxrs-2.1,\
	servlet-5.0,\
	appsecurity-4.0,\
	expressionlanguage-4.0,\
	cdi-3.0,\
	mpOpenAPI-3.0,\
	mpRestClient-3.0,\
	mpMetrics-4.0,\
	mpJwt-2.0,\
	mpConfig-3.0,\
	pages-3.0,\
	jsonb-2.0,\
	servlet-6.0,\
	appsecurity-5.0,\
	expressionlanguage-5.0,\
	cdi-4.0,\
	mpOpenAPI-3.1,\
	mpMetrics-5.0,\
	mpJwt-2.1,\
	pages-3.1,\
	jsonb-3.0,\
	mpconfig-3.1,\
	mpmetrics-5.1

