-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src: \
	fat/src,\
	test-applications/WorkContextJCApp/src,\
	test-resourceadapter/src,\
    test-bundles/threadingtestbundle/src

fat.project: true

# Define additional tested features that are NOT present in any XML files in this bucket.
tested.features:\
	connectors-2.1,\
	mdb-4.0,\
	servlet-6.0,\
	threadingTestFeature-1.0
# above is Not in server.xml, causes timedexit not enabled error

# enable sub-features
-sub: *.bnd

