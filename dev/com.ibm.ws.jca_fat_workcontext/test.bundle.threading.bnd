-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0.0

# *** Testing 03/13/2023 not sure if this is valid.........
# For more information, see http://was.pok.ibm.com/xwiki/bin/view/Liberty/UsingBnd

Bundle-Name: JCA Test Interceptor Bundle
Bundle-SymbolicName: test.bundle.threading; singleton:=true
Bundle-Description: This bundle provides some interceptors to use in testing JCA; version=${bVersion}

# Force the package import versions for all ORB API packages
Import-Package: javax.rmi*;version="[2.4,3)", \
                org.omg.*;version="[2.4,3)", \
                *

#
Export-Package: test.bundle.threading;version=1.0.0

# specify any non-exported packages that need to be bundled
#Private-Package: test.bundle.threading

IBM-Default-Config: OSGI-INF/wlp/defaultInstances.xml

#
Include-Resource: OSGI-INF=resources/OSGI-INF

-dsannotations: test.bundle.threading.ThreadFactoryImpl

#
Include-Resource:\
OSGI-INF/metatype/metatype.xml=test-bundles/threadingtestbundle/resources/OSGI-INF/metatype/metatype.xml,\
OSGI-INF/wlp/defaultInstances.xml=test-bundles/threadingtestbundle/resources/OSGI-INF/wlp/defaultInstances.xml
