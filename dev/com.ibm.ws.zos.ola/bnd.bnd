-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: WebSphere Optimized Local Adapters
Bundle-SymbolicName: com.ibm.ws.zos.ola
Bundle-Description: WebSphere Optimized Local Adapters; version=${bVersion}

WS-TraceGroup: OLA

IBM-Default-Config: OSGI-INF/wlp/defaultInstances.xml

Export-Package: \
  com.ibm.ws390.ola.jca;provide:=true, \
  com.ibm.ws390.ola;provide:=true

Private-Package: com.ibm.ws390.ola.resources

Import-Package: !*.internal.*, !com.ibm.ws.kernel.boot.cmdline, !com.ibm.ws390.ola, *

Include-Resource: \
  OSGI-INF/wlp=resources/OSGI-INF/wlp

instrument.classesExcludes: com/ibm/ws390/ola/resources/*.class

-buildpath: \
	com.ibm.websphere.appserver.api.security;version=latest,\
	com.ibm.websphere.javaee.connector.1.6;version=latest,\
	com.ibm.websphere.javaee.ejb.3.1;version=latest,\
	com.ibm.websphere.ola;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.websphere.security;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.ws.security.authentication;version=latest,\
	com.ibm.ws.security.credentials.saf;version=latest,\
	com.ibm.ws.security;version=latest,\
	com.ibm.ws.zos.channel.wola;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file,\
	com.ibm.ws.junit.extensions;version=latest,\
	org.hamcrest:hamcrest-all;version=1.3,\
	org.jmock:jmock-junit4;strategy=exact;version=2.5.1,\
	org.jmock:jmock;strategy=exact;version=2.5.1,\
	org.jmock:jmock-legacy;version=2.5.0,\
	com.ibm.ws.org.objenesis:objenesis;version=1.0,\
	cglib:cglib-nodep;version=2.1.0.3,\
	com.ibm.ws.kernel.boot;version=latest,\
	com.ibm.ws.kernel.service;version=latest,\
	com.ibm.ws.security.authorization;version=latest,\
	com.ibm.ws.security.registry;version=latest,\
	lib/com.ibm.etools.marshall.runtime_6.1.300.v20130321_2211.jar;version=file
