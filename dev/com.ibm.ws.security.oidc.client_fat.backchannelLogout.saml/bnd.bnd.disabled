-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src: \
  fat/src
  
Import-Package: \
    !*.internal.*, \
    org.jose4j.*;version="[0.5,1)", \
    *
        
fat.project: true

publish.wlp.jar.disabled: true

# added extra features when ee9 was enabled: 
