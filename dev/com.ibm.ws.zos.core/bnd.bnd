-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

# This really is a singleton bundle
Bundle-Name: z/OS Service Integration Core
Bundle-Description: z/OS specific code to link native methods and enable authorized services; version=${bVersion}
Bundle-SymbolicName: com.ibm.ws.zos.core; singleton:=true

WS-TraceGroup: zCore

Export-Package: \
    com.ibm.ws.zos.core;provide:=true, \
    com.ibm.wsspi.zos.command.processing, \
    com.ibm.ws.zos.jni;provide:=true, \
    com.ibm.ws.zos.registration;provide:=true, \
    com.ibm.ws.zos.core.thread;provide:=true, \
    com.ibm.ws.zos.core.structures;provide:=true, \
    com.ibm.ws.zos.core.utils;provide:=true, \
    com.ibm.ws.zos.command.processing.extension;provide:=true

Private-Package: \
    com.ibm.ws.zos.*.internal*, \
    com.ibm.ws.zos.core.diagnostics

Service-Component: \
  com.ibm.ws.zos.core.NativeIntrospection; \
    provide:=com.ibm.wsspi.logging.IntrospectableService; \
    implementation:=com.ibm.ws.zos.core.diagnostics.NativeIntrospection; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    doubleGutter=com.ibm.ws.zos.core.utils.DoubleGutter; \
    bufferHelper=com.ibm.ws.zos.core.utils.DirectBufferHelper; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.core.DoubleGutter; \
    provide:=com.ibm.ws.zos.core.utils.DoubleGutter; \
    implementation:=com.ibm.ws.zos.core.utils.internal.DoubleGutterImpl; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.core.utils.Smf; \
    provide:=com.ibm.ws.zos.core.utils.Smf; \
    implementation:=com.ibm.ws.zos.core.utils.internal.SmfImpl; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.core.structures.NativePsa; \
    provide:=com.ibm.ws.zos.core.structures.NativePsa; \
    implementation:=com.ibm.ws.zos.core.structures.internal.NativePsaImpl; \
    directBufferHelper=com.ibm.ws.zos.core.utils.DirectBufferHelper; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.core.structures.NativeCvt; \
    provide:=com.ibm.ws.zos.core.structures.NativeCvt; \
    implementation:=com.ibm.ws.zos.core.structures.internal.NativeCvtImpl; \
    directBufferHelper=com.ibm.ws.zos.core.utils.DirectBufferHelper; \
    nativePsa=com.ibm.ws.zos.core.structures.NativePsa; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.core.structures.NativeRmct; \
    provide:=com.ibm.ws.zos.core.structures.NativeRmct; \
    implementation:=com.ibm.ws.zos.core.structures.internal.NativeRmctImpl; \
    directBufferHelper=com.ibm.ws.zos.core.utils.DirectBufferHelper; \
    nativeCvt=com.ibm.ws.zos.core.structures.NativeCvt; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.core.structures.NativeRcvt; \
    provide:=com.ibm.ws.zos.core.structures.NativeRcvt; \
    implementation:=com.ibm.ws.zos.core.structures.internal.NativeRcvtImpl; \
    directBufferHelper=com.ibm.ws.zos.core.utils.DirectBufferHelper; \
    nativeCvt=com.ibm.ws.zos.core.structures.NativeCvt; \
