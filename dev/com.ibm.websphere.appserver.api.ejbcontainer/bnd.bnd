-include= ~../cnf/resources/bnd/bundle.props
bVersion: 1.0

Bundle-Name: WebSphere EJB Container API
Bundle-Description: WebSphere EJB Container API, version ${bVersion}
Bundle-SymbolicName: com.ibm.websphere.appserver.api.ejbcontainer

#
# Produce a JakartaEE compliant JAR.
#
jakartaeeMe: true
jakartaFinalJarName: io.openliberty.ejbcontainer.2.0.jar
jakartaFinalBundleName: Jakarta Enterprise Beans Container API
jakartaFinalBundleId: io.openliberty.ejbcontainer.2.0
jakartaFinalBundleDescription: Jakarta Enterprise Beans Container API, version 2.0

Import-Package: \
	com.ibm.websphere.ejbcontainer.mbean,\
	com.ibm.websphere.ejbcontainer

Export-Package: \
	com.ibm.websphere.ejbcontainer.mbean,\
	com.ibm.websphere.ejbcontainer

-includeresource: {META-INF/maven/com.ibm.websphere.appserver.api/com.ibm.websphere.appserver.api.ejbcontainer/pom.xml=com.ibm.websphere.appserver.api.ejbcontainer.pom}

publish.wlp.jar.suffix: dev/api/ibm

-buildpath: \
	com.ibm.ws.ejbcontainer
