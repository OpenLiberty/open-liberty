-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.javaee.websocket.1.0; singleton:=true

Export-Package: \
	javax.websocket; version="1.0.0",\
	javax.websocket.server; version="1.0.0"

Include-Resource: \
  @${repo;javax.websocket:javax.websocket-api;1.0}!/META-INF/LICENSE, \
  @${repo;javax.websocket:javax.websocket-api;1.0}!/META-INF/NOTICE

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	javax.websocket:javax.websocket-api;version=1.0
