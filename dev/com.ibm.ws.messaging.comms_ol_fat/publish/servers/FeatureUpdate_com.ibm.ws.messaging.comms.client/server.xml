<server>
    <!-- TESTING THE UPLOAD! -->
    <featureManager>
        <feature>timedexit-1.0</feature>
        <feature>servlet-3.1</feature>
        <feature>jndi-1.0</feature>
        <feature>wasJmsClient-2.0</feature>
        <feature>ssl-1.0</feature>
    </featureManager>

    <variable name="onError" value="FAIL"/>

    <logging
            traceSpecification="SIBJFapChannel:SIBCommunications:SSLChannel:SSL=all"
            maxFileSize="200"
            maxFiles="2"
    />

    <include location="../fatTestPorts.xml"/>
    <!-- 	<keyStore id="defaultKeyStore" location="key.jks" type="JKS" password="defaultPWD" /> -->
    <keyStore id="defaultKeyStore" location="mykeynew.jks" type="JKS" password="key123"/>


    <jmsQueueConnectionFactory jndiName="jndi_JMS_BASE_QCF" connectionManagerRef="ConMgr1">
        <properties.wasJms userName="user1"
                           password="password123"
                           remoteServerAddress="localhost:${bvt.prop.jms.1.ssl}:BootstrapSecureMessaging"
        />
    </jmsQueueConnectionFactory>
    <connectionManager id="ConMgr1" maxPoolSize="2"/>


    <jmsQueue jndiName="jndi_INPUT_Q">
        <properties.wasJms 	queueName="QUEUE1"
                              timeToLive="500000"
                              busName="defaultBus"   />
    </jmsQueue>
</server>
