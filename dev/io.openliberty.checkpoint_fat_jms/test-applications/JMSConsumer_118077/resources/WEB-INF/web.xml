    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee web-app_3_0.xsd"
    id="WebApp_ID">

  <display-name>JMSConsumer 118077</display-name>

  <servlet id="Default">
    <servlet-name>JMSContext FAT Servlet</servlet-name>
    <display-name>JMSContext FAT Servlet</display-name>
    <servlet-class>jmsconsumer_118077.web.JMSConsumer_118077Servlet</servlet-class>
