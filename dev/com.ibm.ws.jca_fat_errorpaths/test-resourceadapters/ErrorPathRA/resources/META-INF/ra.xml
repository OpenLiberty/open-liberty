<?xml version="1.0" encoding="UTF-8" ?>
<connector xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/connector_1_6.xsd" version="1.6">
  <resourceadapter>
      <inbound-resourceadapter>
      <messageadapter>
        <messagelistener>
          <messagelistener-type>javax.resource.cci.MessageListener</messagelistener-type>
          <activationspec>
            <activationspec-class>com.ibm.test.errorpathadapter.EPActivationSpec</activationspec-class>
            <required-config-property>
              <config-property-name>requiredProp1</config-property-name>
            </required-config-property>
            <required-config-property>
              <config-property-name>requiredProp2</config-property-name>
            </required-config-property>
            <required-config-property>
              <config-property-name>requiredProp3</config-property-name>
            </required-config-property>
            <config-property>
              <config-property-name>requiredProp1</config-property-name>
              <config-property-type>java.lang.String</config-property-type>
            </config-property>
            <config-property>
              <config-property-name>requiredProp3</config-property-name>
              <config-property-type>java.lang.String</config-property-type>
            </config-property>
            <config-property>
              <config-property-name>optionalPropA</config-property-name>
              <config-property-type>java.lang.String</config-property-type>
            </config-property>
          </activationspec>
        </messagelistener>
      </messageadapter>
    </inbound-resourceadapter>
    <adminobject>
      <adminobject-interface>com.ibm.test.errorpathadapter.InterfaceDoesNotExist</adminobject-interface>
      <adminobject-class>com.ibm.test.errorpathadapter.ImplDoesNotExist</adminobject-class>
    </adminobject>
    <adminobject>
      <adminobject-interface>java.util.Date</adminobject-interface>
      <adminobject-class>java.util.Date</adminobject-class>
      <config-property>
        <description>Day of month</description>
        <config-property-name>date</config-property-name> 
        <config-property-type>java.lang.Integer</config-property-type>
        <config-property-value>1</config-property-value> 
      </config-property>
      <config-property>
        <description>Month</description>
        <config-property-name>month</config-property-name> 
        <config-property-type>java.lang.Integer</config-property-type>
        <config-property-value>0</config-property-value> 
      </config-property>
      <config-property>
        <description>Year</description>
        <config-property-name>year</config-property-name> 
        <config-property-type>java.lang.Integer</config-property-type> 
      </config-property>
      <config-property>
        <config-property-name>time</config-property-name> 
        <config-property-type>java.lang.Long</config-property-type>
        <config-property-ignore>true</config-property-ignore> 
      </config-property>
    </adminobject>
    <adminobject>
      <adminobject-interface>java.util.SortedMap</adminobject-interface>
      <adminobject-class>com.ibm.test.errorpathadapter.SortedMap</adminobject-class>
      <config-property>
        <config-property-name>ByteProp1</config-property-name> 
        <config-property-type>java.lang.Byte</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>CharProp1</config-property-name> 
        <config-property-type>java.lang.Character</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>DoubleProp1</config-property-name> 
        <config-property-type>java.lang.Double</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>FloatProp1</config-property-name> 
        <config-property-type>java.lang.Float</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>IntProp1</config-property-name> 
        <config-property-type>java.lang.Integer</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>LongProp1</config-property-name> 
        <config-property-type>java.lang.Long</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>ShortProp1</config-property-name> 
        <config-property-type>java.lang.Short</config-property-type>
      </config-property>
      <config-property>
        <config-property-name>StringProp1</config-property-name> 
        <config-property-type>java.lang.String</config-property-type>
      </config-property>
    </adminobject>
  </resourceadapter>
</connector>