-include= ~../cnf/resources/bnd/bundle.props

bVersion=1.0

# Say that we export these packages to eliminate the Eclipse warnings
Export-Package:\
  com.ibm.websphere.ras;version=1.0,\
  org.apache.tools.ant.taskdefs;version=1.0,\
  test.common.*;version=1.0

publish.wlp.jar.disabled: true
instrument.disabled: true

-buildpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file, \
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.componenttest:public.api;version=1.0.0,\
	org.apache.ant:ant;version=1.9.6,\
	org.apache.ant:ant-junit;version=1.9.6,\
	org.jmock:jmock;version=2.5.1,\
	org.osgi:org.osgi.core;version=6.0.0,\
	org.osgi:org.osgi.service.component;version=1.3.0
