-include= ~../cnf/resources/bnd/bundle.props

-sub: *.bnd

bVersion=1.0

#
# Produce a JakartaEE compliant JAR.
#

WS-TraceGroup: \
    JAVAEESEC

Export-Package: \
	io.openliberty.security.javaeesec.cdi.extensions

Import-Package: \
	!io.openliberty.security.javaeesec.cdi.extensions,\
	!*.internal.*,\
	!com.ibm.ws.kernel.boot.cmdline,\
	*

Private-Package: \
    com.ibm.ws.security.javaeesec.cdi.beans,\
    com.ibm.ws.security.javaeesec.cdi.beans.hash,\
    com.ibm.ws.security.javaeesec.cdi.extensions

-cdiannotations:

-dsannotations: \
    io.openliberty.security.javaeesec.cdi.extensions.JavaEESecCDIExtension

-dsannotations-inherit: true

Include-Resource: \
    META-INF=resources/META-INF

-buildpath: \
    com.ibm.websphere.javaee.annotation.1.2;version=latest,\
    com.ibm.websphere.javaee.cdi.2.0;version=latest,\
    com.ibm.websphere.javaee.el.3.0;version=latest,\
    com.ibm.websphere.javaee.interceptor.1.2;version=latest,\
    com.ibm.websphere.javaee.security.1.0;version=latest,\
    com.ibm.websphere.org.osgi.core,\
    com.ibm.websphere.org.osgi.service.component,\
    com.ibm.wsspi.org.osgi.service.component.annotations,\
    com.ibm.ws.cdi.interfaces;version=latest,\
    com.ibm.ws.container.service;version=latest,\
    com.ibm.ws.logging;version=latest,\
    com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
    com.ibm.ws.security.javaeesec.1.0;version=latest,\
    com.ibm.ws.security.javaeesec.cdi;version=latest,\
    com.ibm.ws.webcontainer.security;version=latest

-testpath: \
    