sslCert.desc=\tUtw\u00f3rz domy\u015blny certyfikat SSL na potrzeby konfiguracji serwera\n\
\tlub klienta. 
#Note to translator the word "createSSLCertificate" should not be translated
sslCert.usage.options=\t{0} createSSLCertificate '{--server=nazwa_serwera |--client=nazwa_klienta'}\n\
\t--password[=has\u0142o] [opcje]
sslCert.required-key.server=\ \ \ \ --server=nazwa
sslCert.required-desc.server=\tSerwer, dla kt\u00f3rego tworzony jest certyfikat.  Tego argumentu nie mo\u017cna     \n\
\tu\u017cy\u0107, je\u015bli u\u017cyto argumentu --client.
sslCert.required-key.client=\ \ \ \ --client=nazwa
sslCert.required-desc.client=\tKlient, dla kt\u00f3rego tworzony jest certyfikat.  Tego argumentu nie mo\u017cna     \n\
\tu\u017cy\u0107, je\u015bli u\u017cyto argumentu --server.
sslCert.required-key.password=\ \ \ \ --password[=has\u0142o]
sslCert.required-desc.password=\tHas\u0142o magazynu kluczy - minimum {1} znak\u00f3w.\n\
\tJe\u015bli warto\u015b\u0107 nie zostanie zdefiniowana, b\u0119dzie wy\u015bwietlana zach\u0119ta.
sslCert.option-key.validity=\ \ \ \ --validity=dni
sslCert.option-desc.validity=\tLiczba dni wa\u017cno\u015bci certyfikatu. Domy\u015blny okres wa\u017cno\u015bci: \n\
\t{2}. Minimalny okres wa\u017cno\u015bci: {3}.
sslCert.option-key.subject=\ \ \ \ --subject=nazwa_wyr\u00f3\u017cniaj\u0105ca
sslCert.option-desc.subject=\tNazwa wyr\u00f3\u017cniaj\u0105ca podmiotu i wystawcy certyfikatu. Domy\u015blna nazwa wyr\u00f3\u017cniaj\u0105ca jest oparta\n \tna nazwie hosta.
sslCert.option.addon=Certyfikat zostanie utworzony z aliasem {4}.\n\
Algorytm klucza: {5}. Algorytm podpisu: {6}.\n\
Aby m\u00f3c lepiej sterowa\u0107 tworzeniem certyfikatu, u\u017cyj bezpo\u015brednio\n\
narz\u0119dzia keytool.
sslCert.option-key.password.encoding=\ \ \ \ --passwordEncoding=[xor|aes]
sslCert.option-desc.password.encoding=\tOkre\u015bl spos\u00f3b kodowania has\u0142a do magazynu kluczy. Obs\u0142ugiwane kodowania\n\
\tto xor i aes. Kodowanie domy\u015blne to xor. \n\
\tU\u017cyj komendy --listCustom kodowania securityUtility, aby sprawdzi\u0107\n\
\tczy s\u0105 dost\u0119pne dodatkowe szyfrowania niestandardowe.
sslCert.option-key.password.key=\ \ \ \ --passwordKey=[klucz]
sslCert.option-desc.password.key=\tOkre\u015bl klucz, kt\u00f3ry ma by\u0107 u\u017cywany do kodowania has\u0142a magazynu\n\
\tkluczy za pomoc\u0105 algorytmu AES. Ten \u0142a\u0144cuch zostanie przetworzony przy\n\
\tu\u017cyciu funkcji mieszaj\u0105cej w celu uzyskania klucza szyfrowania\n\
\tu\u017cywanego do szyfrowania i deszyfrowania has\u0142a. Ten klucz mo\u017cna\n\
\tudost\u0119pni\u0107 na serwerze, definiuj\u0105c zmienn\u0105\n\
\twlp.password.encryption.key, kt\u00f3rej warto\u015bci\u0105 b\u0119dzie ten klucz.\n \tJe\u015bli ta opcja nie zostanie podana, powinien zosta\u0107 u\u017cyty klucz\n\
\tdomy\u015blny.

sslCert.option-key.createConfigFile=\ \ \ \ --createConfigFile=plik
sslCert.option-desc.createConfigFile=\tOpcjonalna. Fragment kodu zostanie zapisany do podanego pliku \n\
\tzamiast na ekran konsoli. Plik ten mo\u017ce zosta\u0107 nast\u0119pnie uwzgl\u0119dniony w\n\
\tkonfiguracji w pliku server.xml za pomoc\u0105 dostarczonego fragmentu kodu.

sslCert.option-key.keySize=\ \ \ \ --keySize=wielko\u015b\u0107
sslCert.option-desc.keySize=\tWielko\u015b\u0107 klucza certyfikatu.  \
Warto\u015bci\u0105 domy\u015bln\u0105 klucza jest {7}.

sslCert.option-key.sigAlg=\ \ \ \ --sigAlg=algorytm podpisu
sslCert.option-desc.sigAlg=\tAlgorytm podpisu dla certyfikatu.\n\
\tDomy\u015blny algorytm podpisu to {8}.


