-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


Bundle-SymbolicName: io.openliberty.jakarta.jaxrs.3.0; singleton:=true

Export-Package: jakarta.ws.rs.*; version="3.0.0.M1"

Import-Package: !jakarta.xml.bind.annotation,\
  !jakarta.xml.bind.annotation.adapters,\
  !javax.xml.namespace,\
  *
  
DynamicImport-Package: jakarta.xml.bind.annotation;version="3.0",\
  jakarta.xml.bind.annotation.adapters;version="3.0",\
  javax.xml.namespace;version="3.0"


instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
  jakarta.ws.rs:jakarta.ws.rs-api;version=3.0.0.M1
