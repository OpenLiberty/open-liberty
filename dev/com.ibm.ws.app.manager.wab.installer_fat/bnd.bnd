-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src: \
	fat/src, \
	test-bundles/product1/src, \
	test-bundles/product2/src, \
	test-bundles/test.wab1/src, \
	test-bundles/test.wab2/src, \
