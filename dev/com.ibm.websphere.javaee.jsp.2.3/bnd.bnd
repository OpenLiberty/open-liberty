-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.javaee.jsp.2.3; singleton:=true

Export-Package: \
	javax.servlet.jsp; version="2.3.0",\
	javax.servlet.jsp.el; version="2.3.0",\
	javax.servlet.jsp.resources; version="2.3.0",\
	javax.servlet.jsp.tagext; version="2.3.0"

Import-Package: !javax.servlet.jsp.*, \
  javax.el; version="3.0.0", \
  javax.servlet.*; version="[2.7.0,3.0.0)", \
  javax.xml.parsers; resolution:=optional, \
  org.apache.taglibs.standard.resources; resolution:=optional, \
  org.apache.taglibs.standard.tag.common.fmt; resolution:=optional, \
  org.xml.sax; resolution:=optional, \
  org.xml.sax.helpers; resolution:=optional

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	com.ibm.ws.javax.j2ee:servlet.jsp.resources;version=2.3, \
    javax.servlet.jsp-api;version=2.3.2.b01
