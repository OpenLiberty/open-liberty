-include=~../cnf/resources/bnd/bundle.props

bVersion=1.0
cxfImportVersion=3.5.5
cxfExportVersion=3.4.3

Bundle-Name: Apache CXF Command Line Tools WADLTo JAXRS Frontend
Bundle-SymbolicName: com.ibm.ws.org.apache.cxf.cxf.tools.wadlto.jaxrs.3.2

Import-Package: \
  !org.apache.ws.commons.schema.*, \
  *

Export-Package: \
  org.apache.cxf.tools.wadlto;version=${cxfExportVersion}, \
  org.apache.cxf.tools.wadlto.jaxb;version=${cxfExportVersion}, \
  org.apache.cxf.tools.wadlto.jaxrs;version=${cxfExportVersion}

Private-Package: schemas.wadl

-includeresource: \
  @${repo;org.apache.cxf:cxf-tools-wadlto-jaxrs;${cxfImportVersion}}!/!META-INF/*

-buildpath: \
  org.apache.cxf:cxf-tools-wadlto-jaxrs;version=${cxfImportVersion}
