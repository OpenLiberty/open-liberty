-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: io.openliberty.jakarta.websocket.2.1; singleton:=true

javac.source: 11
javac.target: 11

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=11))"

Export-Package: \
	jakarta.websocket.server; version="2.1.0"

Include-Resource: \
  @${repo;jakarta.websocket:jakarta.websocket-api;2.1.0}!/META-INF/LICENSE, \
  @${repo;jakarta.websocket:jakarta.websocket-api;2.1.0}!/META-INF/NOTICE

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/spec

-buildpath: \
	jakarta.websocket:jakarta.websocket-api;version=2.1.0
