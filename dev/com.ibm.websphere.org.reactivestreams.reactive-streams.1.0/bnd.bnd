-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.1


Bundle-SymbolicName: com.ibm.websphere.org.reactivestreams.reactive-streams.1.0; singleton:=true


Import-Package: \
  *

Export-Package: \
  org.reactivestreams; version=1.1.0

Include-Resource: \
  @${repo;org.reactivestreams:reactive-streams;1.0.3;EXACT}!/!org/reactivestreams/FlowAdapters*.class, \
  META-INF/versions/9=@${repo;org.reactivestreams:reactive-streams;1.0.3;EXACT}!/org/reactivestreams/FlowAdapters*.class

WS-TraceGroup: REACTIVESTREAMS

Multi-Release: true

instrument.disabled: true

publish.wlp.jar.suffix: dev/api/stable

