-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.appserver.thirdparty.mail
Bundle-Name: WebSphere Mail Third-Party API
Bundle-Description: WebSphere Mail Third-Party API, version ${bVersion}

Export-Package:\
  com.sun.mail;version="1.5.12", \
  com.sun.mail.auth;version="1.5.12", \
  com.sun.mail.imap;version="1.5.0", \
  com.sun.mail.imap.protocol;version="1.5.0", \
  com.sun.mail.iap;version="1.5.0", \
  com.sun.mail.pop3;version="1.5.0", \
  com.sun.mail.smtp;version="1.5.0", \
  com.sun.mail.util;version="1.5.0", \
  com.sun.mail.util.logging;version="1.5.0", \
  com.sun.mail.handlers;version="1.5.0"

Import-Package: \
  javax.activation, \
  javax.mail, \
  javax.mail.event, \
  javax.mail.internet, \
  javax.mail.search, \
  javax.mail.util

publish.wlp.jar.suffix: dev/api/third-party

-buildpath: \
	com.ibm.ws.javamail, \
	com.ibm.ws.com.sun.mail.javax.mail.1.5
