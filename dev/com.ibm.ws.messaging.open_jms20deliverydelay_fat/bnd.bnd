-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src:\
  fat/src,\
  test-applications/DeliveryDelay/src,\
  test-applications/jmsmdb/src

test.project: true

tested.features:\
  jsp-2.3,\
  ejbLite-3.2,\
  wasJmsServer-1.0,\
  wasJmsClient-2.0,\
  wasJmsSecurity-1.0,\
  jmsMdb-3.2,\
  connectors-2.0,\
  pages-3.0,\
  enterpriseBeansLite-4.0,\
  messagingServer-3.0,\
  messagingClient-3.0,\
  messagingSecurity-3.0,\
  mdb-4.0

