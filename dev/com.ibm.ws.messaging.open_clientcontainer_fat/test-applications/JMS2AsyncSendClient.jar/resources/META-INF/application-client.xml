<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application-client
  PUBLIC "-//Sun Microsystems, Inc.//DTD J2EE Application Client 1.2//EN"
         "http://java.sun.com/j2ee/dtds/application-client_1_2.dtd"
>
<application-client id="Application-client_ID">
  <display-name>JMS2AsyncSendClient</display-name>

  <resource-ref>
    <res-ref-name>java:comp/env/jndi_JMS_BASE_QCF</res-ref-name>
    <res-type>javax.jms.QueueConnectionFactory</res-type>
    <lookup-name>jndi_JMS_BASE_QCF</lookup-name>
  </resource-ref>

  <resource-ref>
    <res-ref-name>java:comp/env/jndi_QUEUE_ONE</res-ref-name>
    <res-type>javax.jms.Queue</res-type>
    <lookup-name>jndi_QUEUE_ONE</lookup-name>
  </resource-ref>

  <resource-ref>
    <res-ref-name>java:comp/env/jndi_DEPTH_LIMITED_QUEUE</res-ref-name>
    <res-type>javax.jms.Queue</res-type>
    <lookup-name>jndi_DEPTH_LIMITED_QUEUE</lookup-name>
  </resource-ref>

  <resource-ref>
    <res-ref-name>java:comp/env/topic_raissue</res-ref-name>
    <res-type>javax.jms.Topic</res-type>
    <lookup-name>topic_raissue</lookup-name>
  </resource-ref>

  <resource-ref>
    <res-ref-name>java:comp/env/tcf_raissue</res-ref-name>
    <res-type>javax.jms.TopicConnectionFactory</res-type>
    <lookup-name>tcf_raissue</lookup-name>
  </resource-ref>

  <resource-ref>
    <res-ref-name>java:comp/env/myCF</res-ref-name>
    <res-type>javax.jms.ConnectionFactory</res-type>
  </resource-ref>

</application-client>
