-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Jackson JSON processor 1.6.2
Bundle-SymbolicName:  com.ibm.ws.org.codehaus.jackson.1.6.2
Bundle-Description: Provides Jackson Processor as a bundle; version=${bVersion}

Export-Package: \
	!org.codehaus.jackson.map.ext, \
	org.codehaus.jackson.*

Private-Package: org.codehaus.jackson.map.ext

publish.wlp.jar.disabled: true

-pedantic: false

-buildpath: \
	com.ibm.ws.org.codehaus.jackson:jackson-core-asl;version=1.6.2.1,\
	com.ibm.ws.org.codehaus.jackson:jackson-jaxrs;version=1.6.2.1,\
	com.ibm.ws.org.codehaus.jackson:jackson-mapper-asl;version=1.6.2.1,\
	com.ibm.ws.org.codehaus.jackson:jackson-xc;version=1.6.2.1
