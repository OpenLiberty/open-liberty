<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee web-app_3_0.xsd"
    id="WebApp_ID">

  <display-name>JMSCommsWeb</display-name>

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>COMMS LP Servlet</servlet-name>
    <display-name>COMMS LP Servlet</display-name>
    <servlet-class>com.ibm.ws.messaging.open_comms.fat.CommsLPServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>COMMS LP Servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

  <!-- RESOURCE REFERENCES -->
   <resource-ref>
    <res-ref-name>jndi_JMS_BASE_QCF</res-ref-name>
    <res-type>javax.jms.QueueConnectionFactory</res-type>
    <lookup-name>jndi_JMS_BASE_QCF</lookup-name>
  </resource-ref>  

  <resource-env-ref>
    <resource-env-ref-name>jndi_INPUT_Q</resource-env-ref-name>
    <resource-env-ref-type>javax.jms.Queue</resource-env-ref-type>
  </resource-env-ref>  

</web-app>
