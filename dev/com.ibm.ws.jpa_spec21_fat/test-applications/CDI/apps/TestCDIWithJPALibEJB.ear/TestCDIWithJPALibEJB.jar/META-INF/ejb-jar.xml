        <session>
            <ejb-name>JPACDIWithJPALibSFEJB</ejb-name>
            <business-local>com.ibm.ws.jpa.fvt.cdi.jpalib.ejb.JPACDIWithJPALibSFLocal</business-local>
            <ejb-class>com.ibm.ws.jpa.fvt.cdi.jpalib.ejb.JPACDIWithJPALibSFEJB</ejb-class>
            <session-type>Stateful</session-type>
            <transaction-type>Bean</transaction-type>
        </session>      
    </enterprise-beans>
</ejb-jar>
