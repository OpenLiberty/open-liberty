-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

src: \
  fat/src,\
  test-applications/samltoken/src,\
  test-applications/samlcallerclient/src,\
  test-applications/samlcallertoken/src
  

fat.project: true

tested.features: jsp-2.3, jdbc-4.1, el-3.0, federatedregistry-1.0, ldapregistry-3.0

