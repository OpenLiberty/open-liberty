-include=~../cnf/resources/bnd/bundle.props

-sub: *.bnd

bVersion=1.0
cxfImportVersion=3.5.5
cxfExportVersion=3.4.3

Import-Package:

Export-Package: \
  org.apache.cxf.tools.validator;version=${cxfExportVersion}

-includeresource: \
  @${repo;org.apache.cxf:cxf-tools-validator;${cxfImportVersion};EXACT}!/!META-INF/*

-buildpath: \
  org.apache.cxf:cxf-tools-validator;strategy=exact;version=${cxfImportVersion}
