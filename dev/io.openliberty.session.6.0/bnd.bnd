-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: WAS Session
Bundle-SymbolicName: io.openliberty.session.6.0
Bundle-Description: WAS Session for Servlet 6.0
WLP-ServerName: IBM WebSphere Application Server
WLP-ServerVersion: ${liberty.beta.version}

javac.source: 11
javac.target: 11

# For each exported package, create (in that package) a package-info.java
# file, and place an @version javadoc tag in package-level javadoc. 
# Append ";provide:=true" if this bundle also provides an implementation
# for the exported API.
Export-Package: \
    io.openliberty.session*;provide:=true, \
    io.openliberty.session.http*;provide:=true, \
    io.openliberty.websphere.servlet.session*;provide:=true
	
Import-Package: \
    com.ibm.ws.session.utils, \
    com.ibm.wsspi.session, \
    !*.internal.*, \
    *

instrument.disabled: true

-buildpath: \
	com.ibm.websphere.security;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.logging.core,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
    com.ibm.ws.session.jakarta;version=latest,\
    io.openliberty.jakarta.servlet.6.0;version=latest
