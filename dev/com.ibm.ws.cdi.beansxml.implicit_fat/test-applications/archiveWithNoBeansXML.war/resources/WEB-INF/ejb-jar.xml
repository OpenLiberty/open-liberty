<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	metadata-complete="false" version="3.1">

	<module-name>ArchiveWithNoBeansXml</module-name>
	<display-name>ArchiveWithNoBeansXml</display-name>

    <enterprise-beans>
		<session>
			<ejb-name>SimpleEJB</ejb-name>
			<ejb-class>
				com.ibm.ws.cdi.beansxml.implicit.apps.ejb.ManagedSimpleBean
			</ejb-class>
			<session-type>Stateful</session-type>
		</session>
		<session>
            <ejb-name>OtherSimpleEJB</ejb-name>
            <ejb-class>
                    com.ibm.ws.cdi.beansxml.implicit.apps.ejb.EjbImpl
            </ejb-class>
            <session-type>Stateful</session-type>
        </session>
        <session>
            <ejb-name>FinalEJB</ejb-name>
            <ejb-class>
                    com.ibm.ws.cdi.beansxml.implicit.apps.ejb.EjbImpl
            </ejb-class>
            <session-type>Stateful</session-type>
        </session>
    </enterprise-beans>

</ejb-jar>
