-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: CDI integration for JCache
Bundle-SymbolicName: com.ibm.ws.jcache.cdi
Bundle-Description: CDI integration for JCache providers, version ${bVersion}

Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.8))"

javac.source: 1.8
javac.target: 1.8

Private-Package: com.ibm.ws.jcache.cdi

Include-Resource: \
  META-INF=resources/META-INF

-dsannotations: \
    com.ibm.ws.jcache.cdi.JCacheExtension

-buildpath: \
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.javaee.cdi.2.0;version=latest,\
    com.ibm.websphere.javaee.jcache.1.1;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.ws.cdi.interfaces;version=latest,\
	com.ibm.ws.classloading;version=latest,\
	com.ibm.ws.container.service;version=latest,\
	com.ibm.ws.managedobject;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest
