-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Common JMS Files
Bundle-SymbolicName: com.ibm.ws.messaging.jms.common
Bundle-Description: Common bundle for jms1.1 and jms2.0; version=${bVersion}

Include-Resource: \
  OSGI-INF/metatype=resources/OSGI-INF/metatype,\
  OSGI-INF/l10n=resources/OSGI-INF/l10n

Export-Package: \
  com.ibm.ws.sib.exitpoint.systemcontext,\
  com.ibm.ws.sib.jms.util,\
  com.ibm.ws.sib.pmi.rm,\
  com.ibm.ws.sib.ra,\
  com.ibm.ws.sib.ra.impl,\
  com.ibm.ws.sib.ra.inbound,\
  com.ibm.wsspi.sib.pacing,\
  com.ibm.wsspi.sib.ra,\
  com.ibm.wsspi.util,\
  com.ibm.ws.sib.api.jms.service

Import-Package: \
  !com.ibm.ws.sib.api.jms.service, \
  ${defaultPackageImport}

Service-Component: \
  com.ibm.ws.messaging.jms;\
    implementation:='com.ibm.ws.sib.api.jms.service.JmsServiceFacade';\
    provide:='com.ibm.ws.jca.rar.ResourceAdapterBundleService';\
    configuration-policy:='ignore';\
    modified:='modified';\
    commonServiceFacade='com.ibm.ws.sib.common.service.CommonServiceFacade';\
    immediate:='true';\
    optional:='commonServiceFacade';\
    properties:="type=wasJms"

instrument.disabled: true

-buildpath: \
	com.ibm.websphere.appserver.spi.logging;version=latest,\
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.ws.classloading;version=latest,\
	com.ibm.ws.logging.core;version=latest,\
	com.ibm.ws.messaging.common;version=latest,\
	com.ibm.ws.messaging.utils;version=latest,\
	com.ibm.ws.jca;version=latest,\
	com.ibm.websphere.javaee.connector.1.6;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
