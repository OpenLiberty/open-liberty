Bundle-Description: WebSphere Admin Center Explore Tool, version ${bVersion}

Web-ContextPath: /ibm/adminCenter/explore-1.0
OL-VirtualHost: ${admin.virtual.host}

IBM-Authorization-Roles: com.ibm.ws.management

Include-Resource: \
