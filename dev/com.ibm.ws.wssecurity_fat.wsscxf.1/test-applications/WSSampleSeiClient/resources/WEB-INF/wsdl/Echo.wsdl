                        </wsdl:input>
                        <wsdl:output>
                                <soap:body use="literal" />
                        </wsdl:output>
                </wsdl:operation>
        </wsdl:binding>
        <wsdl:service name="Echo3Service">
                <wsdl:port binding="tns:Echo3SOAP" name="Echo3ServicePort">
                        <soap:address
