<?xml version="1.0" encoding="UTF-8"?>
<webservices version="1.3" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd">	
  <webservice-description>
    <webservice-description-name>HelloService</webservice-description-name>
    <wsdl-file>WEB-INF/wsdl/HelloService.wsdl</wsdl-file>
    <port-component>
	  <port-component-name>Hello</port-component-name>
	  <service-impl-bean>
	      <servlet-link>com.ibm.ws.jaxws.test.endpoint.properties.server.Hello</servlet-link>
	  </service-impl-bean>
    </port-component>
  </webservice-description>
</webservices>