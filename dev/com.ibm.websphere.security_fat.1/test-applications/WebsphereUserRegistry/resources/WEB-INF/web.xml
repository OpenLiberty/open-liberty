<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app id="UserRegistryServlet">
  <display-name>UserRegistry Servlet</display-name>
  <description>This is a servlet designed to drive a UserRegistry instance.</description>

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>UserRegistryServlet</servlet-name>
    <display-name>UserRegistry Servlet</display-name>
    <description>This is a servlet designed to drive a UserRegistry instance.</description>
    <servlet-class>web.UserRegistryServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>UserRegistryServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
