-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Export-Package: \
	com.ibm.ws.kernel.boot.archive.internal

Private-Package: \
	org.apache.commons.compress.archivers, \
	org.apache.commons.compress.archivers.zip, \
	org.apache.commons.compress.archivers.tar, \
