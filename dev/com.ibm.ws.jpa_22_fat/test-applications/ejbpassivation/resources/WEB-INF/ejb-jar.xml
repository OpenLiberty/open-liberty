<ejb-jar id="ejb-jar_id" 
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
        metadata-complete="false" version="3.0">   

	<enterprise-beans>
		<session>
			<ejb-name>StatefulSessionBean</ejb-name>
			<business-local>ejbpassivation.ejb.StatefulSessionBeanLocal</business-local>
			<ejb-class>ejbpassivation.ejb.StatefulSessionBean</ejb-class>
			<session-type>Stateful</session-type>	
		</session>
		<session>
			<ejb-name>StatefulSessionPUBean</ejb-name>
			<business-local>ejbpassivation.ejb.StatefulSessionPUBeanLocal</business-local>
			<ejb-class>ejbpassivation.ejb.StatefulSessionPUBean</ejb-class>
			<session-type>Stateful</session-type>	
		</session>
	</enterprise-beans>
</ejb-jar>
