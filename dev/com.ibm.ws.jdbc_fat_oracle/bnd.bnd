-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


src: \
	fat/src,\
	test-applications/oraclejdbcfat/src,\
	test-applications/oracletracefat/src,\
	test-applications/oracleucpfat/src,\
	test-applications/oraclesslfat/src
	
fat.project: true

