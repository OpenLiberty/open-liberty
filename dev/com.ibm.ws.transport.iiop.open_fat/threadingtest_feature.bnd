-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0.0

#This file defines the user feature bundle. 
Bundle-Name: JCA Test Interceptor Bundle
#Bundle-SymbolicName: test.bundle.threading; singleton:=true
Bundle-SymbolicName: threadingTestFeature; singleton:=true
Bundle-Description: This bundle provides some interceptors to use in testing JCA; version=${bVersion}
# This file defines the threadingTestFeature-1.0 user feature bundle


# 
Export-Package: test.bundle.threading;version=1.0.76

# - specify any non-exported packages that need to be bundled
#Private-Package:  test.bundle.threading.*
Private-Package:  com.ibm.ws.threading.internal.*

# - is this required
Import-Package: \
  javax.enterprise.*; version="[1.1.0,2.1.0)",\
  javax.validation.*; version="[1.1.0,2.1.0)",\
  *

# - LH 5/12/23 to fix the fat.gradle error subprojects/fat.gradle' line: 449
-dependson: \
  build.changeDetector
    
-dsannotations:\
  test.bundle.threading.ThreadFactoryImpl,\
  test.bundle.threading.TaskInterceptorImpl
  
