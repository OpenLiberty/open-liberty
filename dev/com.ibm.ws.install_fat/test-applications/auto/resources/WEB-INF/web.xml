<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app id="WebApp_ID">
  <display-name>Auto</display-name>
  <description>This is a servlet designed to test autoInstall.</description>

  <!-- SERVLET DEFINITIONS -->
  <servlet id="Default">
    <servlet-name>Auto Servlet</servlet-name>
    <display-name>Auto Servlet</display-name>
    <description>Print's to log... that's it!.</description>
    <servlet-class>web.AutoServlet</servlet-class>
    <load-on-startup></load-on-startup>
  </servlet>

  <!-- SERVLET MAPPINGS -->        
  <servlet-mapping id="ServletMapping_Default">
    <servlet-name>Auto Servlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
