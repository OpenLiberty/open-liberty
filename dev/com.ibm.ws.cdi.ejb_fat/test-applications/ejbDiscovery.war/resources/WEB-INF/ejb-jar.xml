<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
	version="3.2">
	<enterprise-beans>
		<session>
			<ejb-name>StatelessDdBean</ejb-name>
			<business-local>com.ibm.ws.cdi.ejb.apps.ejbdiscovery.ejbs.interfaces.StatelessDdLocal</business-local>
			<ejb-class>com.ibm.ws.cdi.ejb.apps.ejbdiscovery.ejbs.StatelessDdBean</ejb-class>
			<session-type>Stateless</session-type>
		</session>

		<session>
			<ejb-name>StatefulDdBean</ejb-name>
			<ejb-class>com.ibm.ws.cdi.ejb.apps.ejbdiscovery.ejbs.StatefulDdBean</ejb-class>
			<session-type>Stateful</session-type>
		</session>

		<session>
			<ejb-name>SingletonDdBean</ejb-name>
			<ejb-class>com.ibm.ws.cdi.ejb.apps.ejbdiscovery.ejbs.SingletonDdBean</ejb-class>
			<session-type>Singleton</session-type>
		</session>

	</enterprise-beans>
</ejb-jar>
