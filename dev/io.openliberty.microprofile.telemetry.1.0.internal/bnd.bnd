app-resources= \
  META-INF/services/jakarta.ws.rs.ext.Providers

-include= ~../cnf/resources/bnd/bundle.props

bVersion=1.0

WS-TraceGroup: telemetry

-cdiannotations:

-dsannotations: \
  io.openliberty.microprofile.telemetry.internal.cdi.SPIMetaData,\
  io.openliberty.microprofile.telemetry.internal.cdi.OpenTelemetryProducer


Include-Resource: \
  META-INF=resources/META-INF

Import-Package: \
	*

Export-Package: \
  io.openliberty.microprofile.telemetry.internal.*;version=1.0;thread-context=true

Private-Package: \
  io.openliberty.microprofile.telemetry.internal.cdi

-buildpath: \
    com.google.guava:failureaccess;version=1.0.1,\
    com.ibm.websphere.appserver.api.servlet;version=latest,\
    io.openliberty.jakarta.servlet.5.0;version=latest,\
    io.openliberty.jakarta.restfulWS.3.0;version=latest,\
    io.openliberty.jakarta.annotation.2.0;version=latest,\
    io.openliberty.jakarta.interceptor.2.0;version=latest,\
    io.openliberty.jakarta.cdi.3.0;version=latest,\
    io.openliberty.microprofile.telemetry.internal.common;version=latest,\
    com.ibm.ws.org.osgi.annotation.versioning;version=latest, \
    com.ibm.ws.cdi.interfaces.jakarta;version=latest,\
    com.ibm.ws.injection;version=latest,\
    com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
    io.openliberty.org.eclipse.microprofile.telemetry.1.0;version=latest, \
    io.openliberty.org.eclipse.microprofile.config.3.0