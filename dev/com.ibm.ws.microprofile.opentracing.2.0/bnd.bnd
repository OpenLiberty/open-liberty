-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


Bundle-Name: com.ibm.ws.microprofile.opentracing.2.0
Bundle-SymbolicName: com.ibm.ws.microprofile.opentracing.2.0
Bundle-Description: MicroProfile.OpenTracing, version 2.0

WS-TraceGroup: OPENTRACING

-cdiannotations:

-dsannotations: \
    com.ibm.ws.microprofile.opentracing.OpenTracingFilterHelper, \
    com.ibm.ws.microprofile.opentracing.cdi.OpenTracingCDIInjectionExtension

Import-Package: \
  !com.ibm.ws.microprofile.opentracing.cdi, \
  javax.enterprise.inject.spi;version="[1.1,2.1)",\
  *

Export-Package: \
	com.ibm.ws.microprofile.opentracing;version=2.0,\
	com.ibm.ws.microprofile.opentracing.cdi;version=2.0


Include-Resource: \
  META-INF=resources/META-INF

-buildpath: \
	com.ibm.ws.opentracing.2.0;version=latest, \
	com.ibm.websphere.javaee.jaxrs.2.0;version=latest, \
	com.ibm.ws.logging;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest, \
	com.ibm.ws.jaxrs.2.0.common;version=latest, \
	com.ibm.wsspi.org.osgi.service.component;version=latest, \
	com.ibm.wsspi.org.osgi.core;version=latest, \
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest, \
	com.ibm.websphere.org.osgi.service.cm;version=latest, \
	com.ibm.websphere.org.eclipse.microprofile.opentracing.2.0;version=latest, \
	com.ibm.websphere.javaee.cdi.1.2;version=latest, \
	com.ibm.ws.cdi.interfaces;version=latest, \
	com.ibm.websphere.javaee.annotation.1.2;version=latest, \
	com.ibm.websphere.javaee.interceptor.1.2;version=latest, \
	com.ibm.ws.io.opentracing.opentracing-api.0.33.0;version=latest, \
	com.ibm.ws.io.opentracing.opentracing-util.0.33.0;version=latest
