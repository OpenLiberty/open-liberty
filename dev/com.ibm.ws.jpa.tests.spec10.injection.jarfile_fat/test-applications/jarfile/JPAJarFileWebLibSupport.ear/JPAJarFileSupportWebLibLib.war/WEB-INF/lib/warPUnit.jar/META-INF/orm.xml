<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">

    <entity class="com.ibm.ws.jpa.commonentities.jpa10.simple.XMLSimpleEntity10">
        <attributes>
            <id name="id"></id>
            <basic name="strData"></basic>
            <basic name="intData"></basic>
            <basic name="longData"></basic>
            
            <basic name="floatData"></basic>
            <basic name="doubleData"></basic>
            <basic name="byteArrData"><lob/></basic>
        </attributes>
    </entity>
</entity-mappings>
