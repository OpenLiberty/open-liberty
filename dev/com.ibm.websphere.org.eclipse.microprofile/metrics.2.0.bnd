-include ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: com.ibm.websphere.org.eclipse.microprofile.metrics.2.0; singleton:=true

Import-Package: \
  javax.enterprise.util; version="[1.1,3)",\
  org.eclipse.microprofile.config; version="[1.0,2)",\
  *

Export-Package: \
  org.eclipse.microprofile.metrics;version=2.0, \
  org.eclipse.microprofile.metrics.annotation;version=2.0

Include-Resource: \
  @${repo;org.eclipse.microprofile.metrics:microprofile-metrics-api;2.0.0;EXACT}

 -maven-dependencies: \
   dep1;groupId=org.eclipse.microprofile.metrics;artifactId=microprofile-metrics-api;version=2.0.0;scope=runtime

WS-TraceGroup: METRICS
