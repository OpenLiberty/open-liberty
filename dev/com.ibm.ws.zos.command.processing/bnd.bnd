-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: z/OS Command Processing Services
Bundle-SymbolicName: com.ibm.ws.zos.command.processing; singleton:=true
Bundle-Description: z/OS Command Processing Services; version=${bVersion}

WS-TraceGroup: zConsole

Private-Package: com.ibm.ws.zos.command.processing.internal*, \
  com.ibm.ws.zos.processing.internal.OSGiCommands, \
  com.ibm.ws.zos.processing.internal.schemagen

-includeresource: \
    OSGI-INF/permissions.perm=resources/permissions.perm

Service-Component: \
  com.ibm.ws.zos.command.processing; \
    implementation:=com.ibm.ws.zos.command.processing.internal.CommandProcessor; \
    kernelCommandLine=com.ibm.ws.kernel.LibertyProcess; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    wsCommandHandler=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    locationAdmin=com.ibm.wsspi.kernel.service.location.WsLocationAdmin; \
    configuration-policy:=ignore; \
    dynamic:='wsCommandHandler'; \
    optional:='wsCommandHandler'; \
    multiple:='wsCommandHandler'; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.command.processing.osgiConsole.OSGiCommandHandler; \
    immediate:=true; \
    implementation:=com.ibm.ws.zos.processing.internal.OSGiCommands.OSGiCommandHandler; \
    provide:=com.ibm.wsspi.zos.command.processing.CommandHandler; \
    properties:="modify.filter.regex=(\(?i\)\(osgi\).*),display.command.help=FALSE,service.vendor=IBM"

instrument.classesExcludes: com/ibm/ws/zos/command/processing/internal/resources/*.class

-buildpath: \
	com.ibm.ws.kernel.boot;version=latest,\
	com.ibm.ws.zos.core;version=latest,\
	org.eclipse.osgi;version=latest,\
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.component;version=latest,\
	com.ibm.websphere.appserver.spi.kernel.service;version=latest,\
	com.ibm.ws.logging.core;version=latest, \
	com.ibm.ws.org.osgi.annotation.versioning;version=latest
    
