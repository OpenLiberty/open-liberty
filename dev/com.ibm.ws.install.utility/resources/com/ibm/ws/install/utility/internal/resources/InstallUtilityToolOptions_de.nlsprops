find.option-desc.[searchString]=\tVerwenden Sie den Suchbegriff, um Assets in den Repositorys zu suchen.
#------------------------------\n at 72 chars -- leading tab-----------\n\#
find.option-key.--showDescriptions=\ \ \ \ --showDescriptions
find.option-desc.--showDescriptions=\tZeigt die Beschreibung f\u00fcr jedes der bei der Suche gefundene Feature an.
#------------------------------\n at 72 chars -- leading tab-----------\n\#
find.option-key.--type=\ \ \ \ --type=[feature|sample|opensource|addon|all*]
find.option-desc.--type=\tSucht Assets des angegebenen Typs.
#------------------------------\n at 72 chars -- leading tab-----------\n\#
find.option-key.--name=\ \ \ \ --name
find.option-desc.--name=\tGibt den Namen des zu suchenden Assets im \n\
\tSuchbegriff an.
#------------------------------\n at 72 chars -- leading tab-----------\n\#
find.option-key.--from=\ \ \ \ --from=Dateipfad
find.option-desc.--from=\tVerwenden Sie diese Option, um den Dateipfad eines einzelnen verzeichnisbasierten\n\
\tRepositorys als einzige Quelle anzugeben, in der installUtility nach Assets suchen soll. Dieser \n\
\tDateipfad kann ein Verzeichnis oder eine Archivdatei sein. 
#------------------------------\n at 72 chars -- leading tab-----------\n\#
find.option-key.--verbose=\ \ \ \ --verbose
find.option-desc.--verbose=\tVerwenden Sie diese Option, wenn alle verf\u00fcgbaren zus\u00e4tzlichen\n\
