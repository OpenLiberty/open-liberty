-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: Jakarta EE API service for Runtime version
Bundle-SymbolicName: io.openliberty.jakartaee.platform.v10
Bundle-Description: Jakarta EE 10.0 API service for runtime, version ${bVersion}


# Register a marker service to enable 10.0 Jakarta EE.
Service-Component: \
  io.openliberty.jakartaee.platform.v10; \
    implementation:=com.ibm.ws.javaee.version.JavaEEVersion; \
    provide:=com.ibm.ws.javaee.version.JavaEEVersion; \
    properties:="version=10.0,service.ranking:Integer=10"
