<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<display-name>Dependency Injection Test</display-name>
	<servlet>
		<servlet-name>InjectInjectionPointXMLServlet</servlet-name>
		<servlet-class>com.ibm.ws.fat.cdi.injectInjectionPointXML.InjectInjectionPointXMLServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InjectInjectionPointXMLServlet</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>10</session-timeout>
	</session-config>
</web-app>
