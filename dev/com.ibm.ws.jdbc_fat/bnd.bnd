-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


src: \
	fat/src,\
	test-resource/loginmodule/src,\
	test-applications/basicfat/src,\
	test-applications/dsdfat/src,\
	test-applications/setupfat/src,\
	test-applications/dsdfat_global_lib/src

fat.project: true
