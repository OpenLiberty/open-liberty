-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: z/OS WLM context service for asynchronous servlets
Bundle-SymbolicName: com.ibm.ws.zos.wlm.context.servlet.async
Bundle-Description: z/OS WLM context service for asynchronous servlets; version=${bVersion}

WS-TraceGroup: zOSWLMContextServletAsync
                
Private-Package: com.ibm.ws.zos.wlm.context.servlet.async.*

-dsannotations: \
