-include= ~../cnf/resources/bnd/bundle.props
bVersion: 1.0

-sub: *.bnd

# All of the sub bundles inherit the following settings.  The gradle logic
# currently doesn't look at sub bundles for the the following two properties
# and only looks at the parent bnd settings.  
# Bnd logic only looks at the buildpath from the parent bnd file for all
# sub bundles.

publish.wlp.jar.suffix: dev/api/stable

instrument.disabled: true

-buildpath: \
  com.ibm.websphere.javaee.annotation.1.3;version=latest,\
  com.ibm.websphere.javaee.cdi.2.0;version=latest,\
  com.ibm.websphere.javaee.interceptor.1.2;version=latest,\
  com.ibm.websphere.javaee.jaxrs.2.1;version=latest,\
  biz.aQute.bnd:biz.aQute.bnd;version=6.3.0,\
  org.eclipse.microprofile.config:microprofile-config-api;version=2.0;strategy=exact,\
  org.eclipse.microprofile.config:microprofile-config-api;version=3.0.2;strategy=exact,\
  org.eclipse.microprofile.context-propagation:microprofile-context-propagation-api;version=1.2;strategy=exact,\
  org.eclipse.microprofile.context-propagation:microprofile-context-propagation-api;version=1.3;strategy=exact,\
  org.eclipse.microprofile.fault-tolerance:microprofile-fault-tolerance-api;version=3.0.0;strategy=exact,\
  org.eclipse.microprofile.fault-tolerance:microprofile-fault-tolerance-api;version=4.0.2;strategy=exact,\
  org.eclipse.microprofile.graphql:microprofile-graphql-api;version=2.0;strategy=exact,\
  org.eclipse.microprofile.health:microprofile-health-api;version=3.0;strategy=exact,\
  org.eclipse.microprofile.health:microprofile-health-api;version=3.1;strategy=exact,\
  org.eclipse.microprofile.health:microprofile-health-api;version=4.0.1;strategy=exact,\
  org.eclipse.microprofile.jwt:microprofile-jwt-auth-api;version=1.2;strategy=exact,\
  org.eclipse.microprofile.jwt:microprofile-jwt-auth-api;version=2.0;strategy=exact,\
  org.eclipse.microprofile.jwt:microprofile-jwt-auth-api;version=2.1;strategy=exact,\
  org.eclipse.microprofile.metrics:microprofile-metrics-api;version=3.0.0;strategy=exact,\
  org.eclipse.microprofile.metrics:microprofile-metrics-api;version=4.0.0;strategy=exact,\
  org.eclipse.microprofile.metrics:microprofile-metrics-api;version=5.0.0;strategy=exact,\
  org.eclipse.microprofile.openapi:microprofile-openapi-api;version=2.0;strategy=exact,\
  org.eclipse.microprofile.openapi:microprofile-openapi-api;version=3.0;strategy=exact,\
  org.eclipse.microprofile.openapi:microprofile-openapi-api;version=3.1;strategy=exact,\
  org.eclipse.microprofile.opentracing:microprofile-opentracing-api;version=2.0.0;strategy=exact,\
  org.eclipse.microprofile.opentracing:microprofile-opentracing-api;version=3.0.0;strategy=exact,\
  org.eclipse.microprofile.rest.client:microprofile-rest-client-api;version=2.0;strategy=exact,\
  org.eclipse.microprofile.rest.client:microprofile-rest-client-api;version=3.0.1;strategy=exact