-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-SymbolicName: io.openliberty.org.eclipse.microprofile.faulttolerance.4.1; singleton:=true

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=11))"

Export-Package: \
  org.eclipse.microprofile.faulttolerance;version=1.2.1, \
  org.eclipse.microprofile.faulttolerance.exceptions;version=1.0

Include-Resource: \
  @${repo;org.eclipse.microprofile.fault-tolerance:microprofile-fault-tolerance-api;4.1.0;EXACT}

-maven-dependencies: \
   dep1;groupId=org.eclipse.microprofile.fault-tolerance;artifactId=microprofile-fault-tolerance-api;version=4.1;scope=runtime

WS-TraceGroup: FAULTTOLERANCE
