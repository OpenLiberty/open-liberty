<application xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
    id="Application_ID">

    <display-name>TxSynchronization</display-name>
        
    <module id="EjbModule_1"> 
        <ejb>TxSynchronizationEJB.jar</ejb>
    </module>
    
    <module id="EjbModule_2"> 
        <ejb>TxSynchronizationBuddyEJB.jar</ejb>
    </module>
    
    <module id="WebModule_1">
        <web>
            <web-uri>TxSynchronization.war</web-uri>
            <context-root>TxSynchronization</context-root>
        </web>
    </module>
    <module id="WebModule_2">
        <web>
            <web-uri>TxSynchronizationEJB.war</web-uri>
            <context-root>TxSynchronizationEJB</context-root>
        </web>
    </module>
</application>
