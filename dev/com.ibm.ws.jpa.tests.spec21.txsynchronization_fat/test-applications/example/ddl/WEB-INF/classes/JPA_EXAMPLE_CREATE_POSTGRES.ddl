CREATE TABLE SimpleExample (id INTEGER NOT NULL, boolean1 BOOL, boolean2 BOOL, boolean3 BOOL, byte1 SMALLINT, byte2 SMALLINT, byte3 SMALLINT, char1 INTEGER, char2 INTEGER, char3 INTEGER, double1 DOUBLE PRECISION, double2 DOUBLE PRECISION, double3 DOUBLE PRECISION, float1 FLOAT4, float2 FLOAT4, float3 FLOAT4, int1 CHAR(1), int2 CHAR(1), int3 CHAR(1), long1 BIGINT, long2 BIGINT, long3 BIGINT, short1 SMALLINT, short2 SMALLINT, short3 SMALLINT, str1 VARCHAR(255), str2 VARCHAR(255), str3 VARCHAR(255), PRIMARY KEY (id));
