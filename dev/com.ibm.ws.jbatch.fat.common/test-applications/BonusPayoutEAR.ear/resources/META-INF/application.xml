<?xml version="1.0" encoding="UTF-8"?>
<application 
    id="Application_ID" 
    version="6" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd">

    <display-name>BonusPayoutEARWithWAR</display-name>
    <module id="BonusPayoutWeb">
        <web>
            <!-- need to change the context-root so as not to collide with the standalone web module, dropins/SimpleBatchJob.war -->
