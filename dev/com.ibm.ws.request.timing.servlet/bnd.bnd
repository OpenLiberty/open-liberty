-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0


Bundle-Name: Request Timing Servlet
Bundle-SymbolicName: com.ibm.ws.request.timing.servlet
Bundle-Description: RequestTimingServlet: Request Timing Servlet; version=${bVersion}

WS-TraceGroup: requestTiming

Private-Package: com.ibm.ws.request.timing.servlet.internal

Include-Resource: \
    OSGI-INF=resources/OSGI-INF

-dsannotations: \
    com.ibm.ws.request.timing.servlet.internal.ConfigParser    

-buildpath: \
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.websphere.org.osgi.service.cm;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
	com.ibm.ws.kernel.boot;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.request.timing;version=latest
