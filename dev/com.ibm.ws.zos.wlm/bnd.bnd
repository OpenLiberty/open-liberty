-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

Bundle-Name: z/OS WLM Services
Bundle-SymbolicName: com.ibm.ws.zos.wlm; singleton:=true
Bundle-Description: z/OS WLM Services; version=${bVersion}

WS-TraceGroup: zOSWLMServices

Import-Package: com.ibm.ws.zos.request.logging.data, *

Export-Package: \
  com.ibm.ws.zos.wlm;provide:=true, \
  com.ibm.ws.zos.wlm.ejb.mdb;provide:=true

Private-Package: \
  com.ibm.ws.zos.wlm.internal*

DynamicImport-Package: com.ibm.wsspi.request.probe.bci

Include-Resource: \
    OSGI-INF/permissions.perm=resources/OSGI-INF/permissions.perm, \
    OSGI-INF=resources/OSGI-INF
    
Service-Component: \
  com.ibm.ws.zos.wlm;\
    provide:='com.ibm.ws.zos.wlm.EnclaveManager,com.ibm.ws.zos.request.logging.data.DataProvider,com.ibm.wsspi.logging.IntrospectableService'; \
    implementation:=com.ibm.ws.zos.wlm.internal.EnclaveManagerImpl;\
    wlmNativeService=com.ibm.ws.zos.wlm.internal.WLMNativeServices; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    configuration-policy:=optional; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.wlm.Authorized_WLMServices; \
    implementation:=com.ibm.ws.zos.wlm.internal.AuthorizedWLMNativeServices; \
    provide:=com.ibm.ws.zos.wlm.internal.WLMNativeServices; \
    wlmConfig=com.ibm.ws.zos.wlm.WLMConfigManager; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    NativeServiceCONNECT='com.ibm.ws.zos.core.NativeService(&(native.service.name=CONNECT)(is.authorized=true))';\
    configuration-policy:=optional; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.wlm.UnauthorizedWLMServices; \
    implementation:=com.ibm.ws.zos.wlm.internal.UnauthorizedWLMNativeServices; \
    provide:=com.ibm.ws.zos.wlm.internal.WLMNativeServices; \
    wlmConfig=com.ibm.ws.zos.wlm.WLMConfigManager; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    NativeServiceCONNECT='com.ibm.ws.zos.core.NativeService(&(native.service.name=CONNECT)(is.authorized=false))';\
    configuration-policy:=optional; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.wlm.WLMHealthImpl; \
    implementation:=com.ibm.ws.zos.wlm.internal.WLMHealthImpl; \
    wlmConfig=com.ibm.ws.zos.wlm.WLMConfigManager; \
    nativeMethodManager=com.ibm.ws.zos.jni.NativeMethodManager; \
    executor=java.util.concurrent.ScheduledExecutorService; \
    configuration-policy:=optional; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.wlm.WLMConfigManager; \
    implementation:=com.ibm.ws.zos.wlm.internal.WLMConfigManagerImpl; \
    provide:=com.ibm.ws.zos.wlm.WLMConfigManager; \
    configuration-policy:=optional; \
    properties:="service.vendor=IBM", \
  com.ibm.ws.zos.wlm.classification.config; \
    provide:='com.ibm.wsspi.http.WorkClassifier,org.osgi.service.cm.ManagedServiceFactory'; \
    implementation:=com.ibm.ws.zos.wlm.internal.WlmClassification; \
    wlmEnclaveManager=com.ibm.ws.zos.wlm.EnclaveManager; \
    executorService=java.util.concurrent.ExecutorService; \
    configurationAdmin=org.osgi.service.cm.ConfigurationAdmin; \
    nativeUtils=com.ibm.ws.zos.core.utils.NativeUtils; \
    mdbClassifier=com.ibm.ws.zos.wlm.ejb.mdb.MDBClassifier; \
    dynamic:='executorService, configurationAdmin, nativeUtils, mdbClassifier'; \
    optional:='executorService, nativeUtils, mdbClassifier'; \
    greedy:='mdbClassifier'; \
    configuration-policy:=ignore; \
    properties:="service.vendor=IBM,service.pid="com.ibm.ws.zos.wlm.classification""

instrument.classesExcludes: com/ibm/ws/zos/wlm/internal/resources/*.class

-buildpath: \
	com.ibm.websphere.org.osgi.core;version=latest,\
	com.ibm.ws.logging;version=latest,\
	com.ibm.ws.zos.core;version=latest,\
	com.ibm.ws.transport.http;version=latest,\
	com.ibm.ws.context;version=latest,\
	com.ibm.websphere.org.osgi.service.cm;version=latest,\
	com.ibm.ws.zos.request.logging.data;version=latest,\
	com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest

-testpath: \
	../build.sharedResources/lib/junit/old/junit.jar;version=file,\
	com.ibm.ws.junit.extensions;version=latest,\
	org.hamcrest:hamcrest-all;version=1.3,\
	org.jmock:jmock-junit4;strategy=exact;version=2.5.1,\
	org.jmock:jmock;strategy=exact;version=2.5.1,\
	org.jmock:jmock-legacy;version=2.5.0,\
	com.ibm.ws.org.objenesis:objenesis;version=1.0,\
	cglib:cglib-nodep;version=2.1.0.3,\
	com.ibm.ws.kernel.boot;version=latest
