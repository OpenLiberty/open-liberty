#!/bin/sh
set -e

case "$1" in
    remove|upgrade)
        echo "Removing all leftover jar files"
        if [ -d "/opt/ol/lib" ]; then
            rm -rf /opt/ol/lib
        fi
        ;;
    abort-install|abort-upgrade|purge)
        ;;
    *)
        echo "postrm called with unknown argument \`$1'" >&2
        ;;
esac

#DEBHELPER#
exit 0

